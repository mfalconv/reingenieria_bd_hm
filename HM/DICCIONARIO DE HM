-- public.usuario definition

-- Drop table

-- DROP TABLE public.usuario;

CREATE TABLE public.usuario (
	id_user int8 NOT NULL,
	estado bool NULL,
	"password" varchar(255) NULL,
	"role" varchar(255) NULL,
	username varchar(255) NOT NULL,
	id_empleado int8 NOT NULL,
	CONSTRAINT usuario_pkey PRIMARY KEY (id_user),
	CONSTRAINT usuario_role_check CHECK (((role)::text = ANY ((ARRAY['ADMIN'::character varying, 'USER'::character varying])::text[])))
);

-- Permissions

ALTER TABLE public.usuario OWNER TO desaweb;
GRANT ALL ON TABLE public.usuario TO desaweb;


