-- DROP TABLE public.n_orden_ocupacional;

CREATE TABLE public.n_orden_ocupacional (
	n_orden int4 DEFAULT nextval('numero_orden'::regclass) NOT NULL,
	cod_pa int4 NULL,
	razon_empresa text NULL,
	razon_contrata text NULL,
	nom_ex text NULL,
	altura_po text NULL,
	mineral_po text NULL,
	fecha_apertura_po date NULL,
	precio_po money NULL,
	estado_ex text DEFAULT 'PROCESO'::text NOT NULL,
	nom_examen text NOT NULL,
	cargo_de text NULL,
	area_o text NULL,
	n_medico text NULL,
	n_hora time NULL,
	tipo_pago text NULL,
	n_fisttest bool NULL,
	n_psicosen bool NULL,
	n_testaltura bool NULL,
	color int4 DEFAULT 0 NULL,
	gruposan text NULL,
	grupofactorsan text NULL,
	cod_clinica text NULL,
	visual_compl bool NULL,
	trab_calientes bool NULL,
	chkcovid1 bool NULL,
	chkcovid2 bool NULL,
	manip_alimentos bool NULL,
	txtobserv1 text NULL,
	txtobserv2 text NULL,
	cod_sede int4 DEFAULT 1 NULL,
	tipo_prueba_covid text DEFAULT 'RA'::text NULL,
	tipoprueba text NULL,
	nombrehotel text NULL,
	protocolo text NULL,
	precio_adic money NULL,
	autoriza text NULL,
	n_operacion text NULL,
	herra_manuales bool NULL,
	rxc_dorso_lumbar bool NULL,
	rxc_lumbar bool NULL,
	rxc_lumbosacra bool NULL,
	rxc_plomos bool NULL,
	mercurioo bool NULL,
	referencia text NULL,
	user_registro text NULL,
	CONSTRAINT n_orden_ocupacional_pkey PRIMARY KEY (n_orden)
);
CREATE INDEX n_razon_contrata ON n_orden_ocupacional USING btree (razon_contrata);
CREATE INDEX n_razon_empresa ON n_orden_ocupacional USING btree (razon_empresa);

-- Permissions

ALTER TABLE public.n_orden_ocupacional OWNER TO postgres;
GRANT ALL ON TABLE public.n_orden_ocupacional TO postgres;


