 	declare  genero text;
	BEGIN

if(sexo='MASCULINO')
THEN
SET genero='M';
END IF;
if (sexo='FEMENINO')
THEN
SET genero='M';
END IF;
 verificarcod:=(select cod_pa from datos_paciente where cod_pa=codecode);
END;
$function$
;

