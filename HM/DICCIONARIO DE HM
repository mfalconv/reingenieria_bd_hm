DICCIONARIO DE HM 

-- DROP SCHEMA public;

CREATE SCHEMA public AUTHORIZATION postgres;

COMMENT ON SCHEMA public IS 'standard public schema';

-- DROP SEQUENCE public.archivos_servidores_seq;

CREATE SEQUENCE public.archivos_servidores_seq
	INCREMENT BY 50
	MINVALUE 1
	MAXVALUE 9223372036854775807
	START 1
	NO CYCLE;

-- Permissions

ALTER SEQUENCE public.archivos_servidores_seq OWNER TO desaweb;
GRANT ALL ON SEQUENCE public.archivos_servidores_seq TO desaweb;

-- DROP SEQUENCE public.b_c_vehiculos;

