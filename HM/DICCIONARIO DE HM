-- DROP FUNCTION public.spsumaservicios(numeric);

CREATE OR REPLACE FUNCTION public.spsumaservicios(codigo numeric)
 RETURNS integer
 LANGUAGE plpgsql
AS $function$
declare var integer;
begin
var=(select SUM(preciocobrar) from historialClienteProtocolo where n_orden=codigo);
RETURN var;

end
$function$
;

-- Permissions

ALTER FUNCTION public.spsumaservicios(numeric) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spsumaservicios(numeric) TO huanchaco;

-- DROP FUNCTION public.spsumaservicios(int4);

CREATE OR REPLACE FUNCTION public.spsumaservicios(codigo integer)
 RETURNS numeric
 LANGUAGE plpgsql
AS $function$
declare var numeric(10,2);
declare preciop numeric(10,2);
begin
preciop=(select pt.preciototal from historialClienteProtocolo as hcp left join protocoloServicio as ps on hcp.idprotocoloservicio
=ps.idprotocoloservicio left join protocolo as pt on ps.idprotocolo=pt.idprotocolo 
where hcp.n_orden=codigo limit 1);
if(preciop>0)
then
var=preciop;
else
var=(select SUM(preciocobrar) from historialClienteProtocolo where n_orden=codigo);
end if;
RETURN var;

end
$function$
;

-- Permissions

ALTER FUNCTION public.spsumaservicios(int4) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spsumaservicios(int4) TO huanchaco;

-- DROP FUNCTION public.validar_reporte_alicel(int8);

CREATE OR REPLACE FUNCTION public.validar_reporte_alicel(p_n_orden bigint)
 RETURNS text
 LANGUAGE plpgsql
AS $function$
DECLARE
    resultado INT := 0;
BEGIN
    -- Verificar el conteo
    SELECT COUNT(ap.n_orden)
    INTO resultado
    FROM antecedentes_patologicos ap
    JOIN n_orden_ocupacional no ON ap.n_orden = no.n_orden
    WHERE no.razon_empresa = 'INVERSIONES Y SERVICIOS ALICEL S.A.C.'
      AND ap.n_orden = p_n_orden;

    -- Depurar el resultado
    RAISE NOTICE 'Resultado del conteo: %', resultado;

    -- Lgica de retorno
    IF resultado >= 1 THEN
        RETURN 'ficha_antecedente_patologico_Digitalizado_Alicel.jasper';
    ELSE
        RETURN 'ficha_antecedente_patologico_Digitalizado.jasper';
    END IF;

EXCEPTION
    WHEN OTHERS THEN
        -- Mostrar el error exacto
        RAISE NOTICE 'Error interno: %', SQLERRM;
        RETURN 'error';
END;
$function$
;

-- Permissions

ALTER FUNCTION public.validar_reporte_alicel(int8) OWNER TO pierola;
GRANT ALL ON FUNCTION public.validar_reporte_alicel(int8) TO pierola;

-- DROP FUNCTION public.validar_reporte_alicel(int8, text);

CREATE OR REPLACE FUNCTION public.validar_reporte_alicel(p_n_orden bigint, p_nombre_tabla text)
 RETURNS text
 LANGUAGE plpgsql
AS $function$
DECLARE
    resultado INT := 0; -- Variable para almacenar el resultado
BEGIN
    -- Validar si la tabla es 'antecedentes patologicos'
    IF p_nombre_tabla = 'antecedentes patologicos' THEN
        -- Verificar existencia del n_orden en ambas tablas y empresa
        SELECT CASE
            WHEN COUNT(*) > 0 THEN 1
            ELSE 0
        END
        INTO resultado
        FROM antecedentes_patologicos ap
        JOIN n_orden_ocupacional no ON ap.n_orden = no.n_orden
        WHERE no.razon_empresa = 'INVERSIONES Y SERVICIOS ALICEL S.A.C.'
          AND ap.n_orden = p_n_orden;
    END IF;

    -- Retornar el reporte segn el resultado
    IF resultado >= 1 THEN
        RETURN 'jasper_alicel';
    ELSE
        RETURN 'jasper_normal';
    END IF;

EXCEPTION
    WHEN OTHERS THEN
        -- Manejo de errores
        RAISE NOTICE 'Error ejecutando la funcin: %', SQLERRM;
        RETURN 'error';
END;
$function$
;

-- Permissions

ALTER FUNCTION public.validar_reporte_alicel(int8, text) OWNER TO pierola;
GRANT ALL ON FUNCTION public.validar_reporte_alicel(int8, text) TO pierola;


