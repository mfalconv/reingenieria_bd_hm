	colesterol varchar(255) NULL,
	combinado varchar(255) NULL,
	conc_diagnostica varchar(255) NULL,
	conclucion_creatinina varchar(255) NULL,
	conclucion_hemoglobina varchar(255) NULL,
	conclusionesaptitud varchar(255) NULL,
	conclusionodo varchar(255) NULL,
	creatinina varchar(255) NULL,
	dermatologico varchar(255) NULL,
	diagnostico_musculo_esqueletico varchar(255) NULL,
	dir varchar(255) NULL,
	dni int4 NULL,
	ectoscopia varchar(255) NULL,
	edad_pac int4 NULL,
	ekg_resultado varchar(255) NULL,
	entregado varchar(255) NULL,
	espacios_confinados varchar(255) NULL,
	espirometria varchar(255) NULL,
	estado varchar(255) NULL,
	estasis varchar(255) NULL,
	estereopsis varchar(255) NULL,
	factor varchar(255) NULL,
	fc varchar(255) NULL,
	fecha_inicio varchar(255) NULL,
	fecha_nacimiento_pac varchar(255) NULL,
	fecha_salida varchar(255) NULL,
	fechavence varchar(255) NULL,
	fondo_ojo varchar(255) NULL,
	fosfatasaalcaina varchar(255) NULL,
	fr varchar(255) NULL,
	genero varchar(255) NULL,
	ggtp varchar(255) NULL,
	glb varchar(255) NULL,
	glucosa varchar(255) NULL,
	grupo varchar(255) NULL,
	hcm varchar(255) NULL,
	hdl_colesterol varchar(255) NULL,
	hemoglobina varchar(255) NULL,
	imc varchar(255) NULL,
	ind varchar(255) NULL,
	ishihara varchar(255) NULL,
	ldl_colesterol varchar(255) NULL,
	lejos_corregida_od varchar(255) NULL,
	lejos_corregida_oi varchar(255) NULL,
	lejos_od varchar(255) NULL,
	lejos_oi varchar(255) NULL,
	lumbar varchar(255) NULL,
	metanfetaminas varchar(255) NULL,
	mieolocitosis varchar(255) NULL,
	neurologico varchar(255) NULL,
	nictometria varchar(255) NULL,
	nombres varchar(255) NULL,
	osteomuscular varchar(255) NULL,
	p_abdominal varchar(255) NULL,
	peso varchar(255) NULL,
	plazodias varchar(255) NULL,
	presionarterial varchar(255) NULL,
	promielocitos varchar(255) NULL,
	prueba_esfuerso varchar(255) NULL,
	r_imc varchar(255) NULL,
	rango varchar(255) NULL,
	razon_contrata varchar(255) NULL,
	razon_empresa varchar(255) NULL,
	recomendacionesaptitud varchar(255) NULL,
	restriccionesaptitud varchar(255) NULL,
	resultadodeevaluaciones varchar(255) NULL,
	riesgo_coronario varchar(255) NULL,
	sat_02 varchar(255) NULL,
	seguimiento_observaciones varchar(255) NULL,
	status varchar(255) NULL,
	talla varchar(255) NULL,
	temperatura varchar(255) NULL,
	tgo varchar(255) NULL,
	tgp varchar(255) NULL,
	tipodeemo varchar(255) NULL,
	torax varchar(255) NULL,
	total varchar(255) NULL,
	total2 varchar(255) NULL,
	trigliceridos varchar(255) NULL,
	txtabastonados varchar(255) NULL,
	txtantecedentesfamiliares varchar(255) NULL,
	txtantecedentespersonales varchar(255) NULL,
	txtausentes varchar(255) NULL,
	txtbacteriassu varchar(255) NULL,
	txtcariadasoturar varchar(255) NULL,
	txtcelepitelialessu varchar(255) NULL,
	txtcocaina varchar(255) NULL,
	txtcristalessu varchar(255) NULL,
	txteosinofiosematologia varchar(255) NULL,
	txthematiesematologia varchar(255) NULL,
	txthematiessu varchar(255) NULL,
	txthematocrito varchar(255) NULL,
