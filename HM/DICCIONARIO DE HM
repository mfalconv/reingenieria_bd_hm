	email_pa text NULL,
	lugar_nac_pa text NULL,
	nivel_est_pa text NULL,
	ocupacion_pa text NULL,
	estado_civil_pa text NULL,
	direccion_pa text NULL,
	departamento_pa text NULL,
	provincia_pa text NULL,
	distrito_pa text NULL,
	caserio_pa text NULL,
	foto_pa bytea NULL,
	cod_aleatorio_pa int4 NULL,
	historial_pa int4 DEFAULT nextval('historial_pacientes'::regclass) NOT NULL,
	tel_casa_pa text NULL,
	tel_trabajo_pa text NULL,
	cel_pa text NULL,
	fecha_registro_pa date NULL,
	apellidos_pa text NULL,
	hora_registro_pa text NULL,
	tipo_doc int4 DEFAULT 1 NULL
);

