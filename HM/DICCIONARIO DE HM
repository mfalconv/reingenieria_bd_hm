-- DROP FUNCTION public.sp_pruebagrid2();

CREATE OR REPLACE FUNCTION public.sp_pruebagrid2()
 RETURNS SETOF record
 LANGUAGE plpgsql
AS $function$
declare
tabla record;
begin
for tabla in 
	SELECT a.razon_contrata,b.nombres_pa,b.apellidos_pa,a.fecha_apertura_po,a.estado_ex,a.nom_examen,a.cargo_de,a.area_o
		FROM n_orden_ocupacional a
		inner join datos_paciente b on b.cod_pa=a.cod_pa
		where a.razon_empresa='LA ARENA S.A.' 
loop
return next tabla;
end loop;
return;
end
$function$
;

-- Permissions

ALTER FUNCTION public.sp_pruebagrid2() OWNER TO postgres;
GRANT ALL ON FUNCTION public.sp_pruebagrid2() TO postgres;

-- DROP FUNCTION public.spagregarconta(int4);

CREATE OR REPLACE FUNCTION public.spagregarconta(norden integer)
 RETURNS void
 LANGUAGE plpgsql
AS $function$
BEGIN

INSERT INTO contabilidad 
(select norden, c.c_n_factura, current_Date, current_Date, c.c_monto_fac, 
            c.c_num_ope, c.c_banco,c.c_cuenta, c.c_estado, current_Date, c.c_observaciones
from contabilidad as c) limit 1;
END;
$function$
;

-- Permissions

ALTER FUNCTION public.spagregarconta(int4) OWNER TO postgres;
GRANT ALL ON FUNCTION public.spagregarconta(int4) TO postgres;

-- DROP FUNCTION public.spagregarpe(int4);

CREATE OR REPLACE FUNCTION public.spagregarpe(norden integer)
 RETURNS void
 LANGUAGE plpgsql
AS $function$
	Declare tipoP text;
	Declare dnis integer;
BEGIN
tipoP=(select tipoprueba from n_orden_ocupacional  where n_orden=norden);
if(tipoP='AE')
then
dnis=(select noo.cod_pa from n_orden_ocupacional as noo inner join datos_paciente as dt on
	  noo.cod_pa=dt.cod_pa where noo.n_orden=norden);
INSERT INTO examen_inmunologico 
(select norden,ei.dni_pa,current_Date,ei.chkigm_reactivo,ei.chkigm_noreactivo,ei.chkigg_reactivo,
ei.chkigg_noreactivo,ei.chkinvalido,ei.txtobservaciones,ei.cbomarca,ei.txtvrigm,ei.txtvrigg
from n_orden_ocupacional as noo
inner join examen_inmunologico as ei on noo.n_orden=ei.n_orden
inner join datos_paciente as dp on noo.cod_pa=dp.cod_pa  where noo.cod_pa=dnis
 and tipoprueba<>'AE' order by noo.n_orden desc limit 1);
end if;
END;
$function$
;

-- Permissions

ALTER FUNCTION public.spagregarpe(int4) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spagregarpe(int4) TO huanchaco;

-- DROP FUNCTION public.spfuncioncategoria(int4, text, int4, int4, text);

CREATE OR REPLACE FUNCTION public.spfuncioncategoria(codecode integer, namename text, ord integer, operacion integer, user_reg text)
 RETURNS void
 LANGUAGE plpgsql
AS $function$
BEGIN
    CASE operacion 
        WHEN 1 THEN          
           
            INSERT INTO categoria (
                nombrecategoria,                  
                orden,                 
                user_registro
            ) VALUES (
                namename, 
                ord, 
                user_reg );
            RAISE NOTICE 'Datos guardados con xito';

        WHEN 2 THEN
            
            UPDATE categoria
            SET 
                nombrecategoria = namename,
                orden = ord,               
                user_actualizacion = user_reg
            WHERE idcat = codecode;
            RAISE NOTICE 'Actualizacin realizada correctamente';

        WHEN 3 THEN
            
            DELETE FROM categoria 
            WHERE idcat = codecode;
            RAISE NOTICE 'Eliminado correctamente';

        ELSE            
            RAISE EXCEPTION 'Operacin no vlida: %', operacion;
    END CASE;
END;
$function$
;

-- Permissions

ALTER FUNCTION public.spfuncioncategoria(int4, text, int4, int4, text) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spfuncioncategoria(int4, text, int4, int4, text) TO huanchaco;

-- DROP FUNCTION public.spfuncionhistorialclienteprotocolo(int4, int4, int4);

CREATE OR REPLACE FUNCTION public.spfuncionhistorialclienteprotocolo(codecode integer, pkprotocolo integer, operacion integer)
 RETURNS void
 LANGUAGE plpgsql
AS $function$
declare el_dni integer;
declare la_edad integer;
declare fecha_nac date;
declare fecha_actuall date;
BEGIN
 fecha_actuall=(select date(now()));
 el_dni=(select cod_pa from n_orden_ocupacional where  n_orden=codecode);
 fecha_nac=(select fecha_nacimiento_pa from datos_paciente where cod_pa=el_dni);
 la_edad=(select obtener_edad(fecha_nac,fecha_actuall));
if(operacion =1)
then
insert into historialClienteProtocolo (n_orden,idprotocoloservicio,preciocobrar) 
select codecode,idProtocoloServicio,(case  when 
				((select nombreservicio from servicios where idservicio =s.idServicio  and 
upper(nombreservicio) like  '%CONDICIONAL%') IS NULL)					 
									 then s.precioreal 
					when
((select nombreservicio from servicios where idservicio =s.idServicio  and 
 upper(nombreservicio) like '%CONDICIONAL%') IS NOT NULL	and la_edad>=ps.edad)
									 then s.precioreal 
									 else 0
									 end) 
from protocoloServicio as ps  inner join servicios as s on ps.idservicio=s.idservicio 
where idProtocolo=pkprotocolo;
end if;

if(operacion=2)
then
delete from historialClienteProtocolo where n_orden=codecode;
insert into historialClienteProtocolo (n_orden,idprotocoloservicio,preciocobrar) 
select codecode,idProtocoloServicio,(case  when 
				((select nombreservicio from servicios where idservicio =s.idServicio  and 
upper(nombreservicio) like  '%CONDICIONAL%') IS NULL)					 
									 then s.precioreal 
					when
((select nombreservicio from servicios where idservicio =s.idServicio  and 
upper(nombreservicio) like  '%CONDICIONAL%') IS NOT NULL	and la_edad>=ps.edad)
									 then s.precioreal 
									 else 0
									 end) 
from protocoloServicio as ps  inner join servicios as s on ps.idservicio=s.idservicio 
where idProtocolo=pkprotocolo;
end if;

END;
$function$
;

-- Permissions

ALTER FUNCTION public.spfuncionhistorialclienteprotocolo(int4, int4, int4) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spfuncionhistorialclienteprotocolo(int4, int4, int4) TO huanchaco;

-- DROP FUNCTION public.spfuncionhoteloperaciones(int4, text, text, text, bit, int4);

CREATE OR REPLACE FUNCTION public.spfuncionhoteloperaciones(codecode integer, namename text, diredire text, coment text, estest bit, operacion integer)
 RETURNS void
 LANGUAGE plpgsql
AS $function$
BEGIN
if(operacion =1)
then
insert into InfoHoteles(nombreHotel,direccion,comentarios, estado) values(namename,diredire,coment,estest);
end if;
if(operacion=2)
then
update InfoHoteles set nombreHotel=namename, direccion=diredire,comentarios=coment,estado=estest where idHotel=codecode;
end if;
if(operacion=3)
then
delete from InfoHoteles where idHotel=codecode;
end if;

END;
$function$
;

-- Permissions

ALTER FUNCTION public.spfuncionhoteloperaciones(int4, text, text, text, bit, int4) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spfuncionhoteloperaciones(int4, text, text, text, bit, int4) TO huanchaco;

-- DROP FUNCTION public.spfuncionllenadopc(int4, date, text);

CREATE OR REPLACE FUNCTION public.spfuncionllenadopc(elorden integer, feecha date, satu text)
 RETURNS void
 LANGUAGE plpgsql
AS $function$
BEGIN
INSERT INTO fmedica_covid_marsa (n_orden,fecha,fecha_exra,txtrecomendaciones)values(elOrden,feecha,feecha,'llenado en laboratorio clinico');
insert into triaje (n_orden,fecha_triaje,sat_02,conclusion) values(elOrden,feecha,satu,'llenado en laboratorio clinico por ser pc');
insert into constancia_salud_marsa (n_orden,fecha_examen,hora_examen,txtmuestra) values(elOrden,feecha,current_timestamp,'muestra Pc');
END;
$function$
;

-- Permissions

ALTER FUNCTION public.spfuncionllenadopc(int4, date, text) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spfuncionllenadopc(int4, date, text) TO huanchaco;

-- DROP FUNCTION public.spfuncionprotocolos(int4, text, int4, text, numeric);

CREATE OR REPLACE FUNCTION public.spfuncionprotocolos(codecode integer, namename text, operacion integer, user_regis text, precio_total numeric)
 RETURNS void
 LANGUAGE plpgsql
AS $function$
BEGIN
    IF operacion = 1 THEN       
        INSERT INTO protocolo (nombreprotocolo, user_registro,  preciototal)
        VALUES (namename, user_regis, precio_total);
        RAISE NOTICE 'Datos guardados con xito';

    ELSIF operacion = 2 THEN
        
        IF EXISTS (SELECT 1 FROM protocolo WHERE idprotocolo = codecode) THEN
            UPDATE protocolo
            SET 
            nombreprotocolo = namename,       
            user_actualizacion = user_regis, 
            preciototal = precio_total
            WHERE idprotocolo = codecode;
            RAISE NOTICE 'Actualizacin realizada correctamente';
        ELSE
            RAISE EXCEPTION 'No se encontr el protocolo con ID %', codecode;
        END IF;

    ELSIF operacion = 3 THEN
        
        IF EXISTS (SELECT 1 FROM protocolo WHERE idprotocolo = codecode) THEN
            DELETE FROM protocolo WHERE idprotocolo = codecode;
            RAISE NOTICE 'Eliminado correctamente';
        ELSE
            RAISE EXCEPTION 'No se encontr el protocolo con ID % para eliminar', codecode;
        END IF;

    ELSE
        RAISE EXCEPTION 'Operacin no vlida: %', operacion;
    END IF;
END;
$function$
;

-- Permissions

ALTER FUNCTION public.spfuncionprotocolos(int4, text, int4, text, numeric) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spfuncionprotocolos(int4, text, int4, text, numeric) TO huanchaco;

-- DROP FUNCTION public.spfuncionprotocoloservicio(int4, int4, int4, numeric, int4, text);

CREATE OR REPLACE FUNCTION public.spfuncionprotocoloservicio(codecode integer, idproto integer, idserv integer, ord numeric, operacion integer, user_regi text)
 RETURNS void
 LANGUAGE plpgsql
AS $function$
BEGIN
    IF operacion = 1 THEN
        
        INSERT INTO protocoloServicio (idProtocolo,  idServicio, orden, user_registro ) 
        VALUES ( idproto, idserv, ord, user_regi);
        RAISE NOTICE 'Datos guardados con xito';

    ELSIF operacion = 2 THEN
        
        IF EXISTS (SELECT 1 FROM protocoloServicio WHERE idProtocoloServicio = codecode) THEN
            UPDATE protocoloServicio
            SET 
                idProtocolo = idproto,
                idServicio = idserv,
                orden = ord,         
                user_actualizacion = user_regi
            WHERE idProtocoloServicio = codecode;
            RAISE NOTICE 'Actualizacin realizada correctamente';
        ELSE
            RAISE EXCEPTION 'No se encontr el protocolo de servicio con ID %', codecode;
        END IF;

    ELSIF operacion = 3 THEN      
        -- Verificar existencia antes de eliminar
        IF EXISTS (SELECT 1 FROM protocoloServicio WHERE idProtocoloServicio = codecode) THEN
            DELETE FROM protocoloServicio WHERE idProtocoloServicio = codecode;
            RAISE NOTICE 'Eliminado correctamente';
        ELSE
            RAISE EXCEPTION 'No se encontr el protocolo de servicio con ID % para eliminar', codecode;
        END IF;

    ELSE
        RAISE EXCEPTION 'Operacin no vlida: %', operacion;
    END IF;
END;
$function$
;

-- Permissions

ALTER FUNCTION public.spfuncionprotocoloservicio(int4, int4, int4, numeric, int4, text) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spfuncionprotocoloservicio(int4, int4, int4, numeric, int4, text) TO huanchaco;

-- DROP FUNCTION public.spfuncionregistroautomaticopaciente(date, text, int4, text, text, text, date, text, text, text, text, text, text, text, text, text, text, text, text, text);

CREATE OR REPLACE FUNCTION public.spfuncionregistroautomaticopaciente(fechaexamen date, tipodoc text, codecode integer, nombrepac text, apellidopa text, apellidomat text, fechanaci date, sexo text, email text, lugarnac text, nivelestudio text, ocupacion text, estadocivil text, direccion text, departamento text, provincia text, distrito text, caserio text, numfijo text, cuncel text)
 RETURNS void
 LANGUAGE plpgsql
AS $function$
	declare verificarcod integer;
 	declare  genero text;
BEGIN

if(sexo='MASCULINO')
THEN
 genero='M';
END IF;
if (sexo='FEMENINO')
THEN
 genero='F';
END IF;
 verificarcod=(select cod_pa from datos_paciente where cod_pa=codecode);
if (verificarcod is null)
then
Insert INTO datos_paciente (cod_pa,nombres_pa,apellidos_pa,fecha_nacimiento_pa,sexo_pa,email_pa,
							lugar_nac_pa,nivel_est_pa,ocupacion_pa,estado_civil_pa,direccion_pa,
							departamento_pa,provincia_pa,distrito_pa,caserio_pa,tel_casa_pa,
							cel_pa,fecha_registro_pa,hora_registro_pa,tipo_doc)
values(codecode,nombrePac,CONCAT(apellidoPa,' ',apellidoMat),fechaNaci,genero,email,lugarNac,nivelEstudio,
								 ocupacion,estadoCivil,direccion,departamento,provincia,distrito,
								 caserio,numFijo,cunCel,current_Date,current_time,
	   						     (case when tipoDoc='DNI' then 1 else 2 end));
end if;
--INSERT INTO n_orden_ocupacional(cod_pa, razon_empresa, razon_contrata, 
   --                     nom_ex, altura_po,mineral_po, fecha_apertura_po,nom_examen,precio_po,
	--				   cargo_de,n_medico,n_hora,area_o,tipo_pago)
--values(codecode,razonEmpresa,razonContratista,explotacion,alturaLaboral,mineral,current_Date,nombreExamen,
--	   'S/.0.00',puestoTrabajo,'FRANCK JHONATAN MESTAS VEGA',current_time,area,'EFECTIVO');
END;
$function$
;

-- Permissions

ALTER FUNCTION public.spfuncionregistroautomaticopaciente(date, text, int4, text, text, text, date, text, text, text, text, text, text, text, text, text, text, text, text, text) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spfuncionregistroautomaticopaciente(date, text, int4, text, text, text, date, text, text, text, text, text, text, text, text, text, text, text, text, text) TO huanchaco;

-- DROP FUNCTION public.spfuncionsedeoperaciones(int4, text, text, text, bit, int4);

CREATE OR REPLACE FUNCTION public.spfuncionsedeoperaciones(codecode integer, namename text, diredire text, coment text, estest bit, operacion integer)
 RETURNS void
 LANGUAGE plpgsql
AS $function$
BEGIN
if(operacion =1)
then
insert into sede(nombre_sede,direccion,comentarios, estado) values(namename,diredire,coment,estest);
end if;
if(operacion=2)
then
update sede set nombre_sede=namename, direccion=diredire,comentarios=coment,estado=estest where cod_sede=codecode;
end if;
if(operacion=3)
then
delete from sede where cod_sede=codecode;
end if;

END;
$function$
;

-- Permissions

ALTER FUNCTION public.spfuncionsedeoperaciones(int4, text, text, text, bit, int4) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spfuncionsedeoperaciones(int4, text, text, text, bit, int4) TO huanchaco;

-- DROP FUNCTION public.spfuncionservicios(int4, text, int4, numeric, numeric, numeric, int4, text, bool, text);

CREATE OR REPLACE FUNCTION public.spfuncionservicios(codecode integer, nameserv text, cat integer, orde numeric, pb numeric, pr numeric, operacion integer, observ text, estad boolean, user_reg text)
 RETURNS void
 LANGUAGE plpgsql
AS $function$
BEGIN
    CASE operacion 
        WHEN 1 THEN
            -- Insercin
            INSERT INTO servicios (
                nombreServicio,
                categoria_servi,
                orden, 
                precioBase, 
                precioReal, 
                observaciones,                            
                estado_servi,  -- Nuevo campo booleano despus de observ
                user_registro
            ) VALUES (
                nameserv, 
                cat,
                orde,  
                pb, 
                pr, 
                observ, 
                estad,  -- Valor del nuevo campo booleano
                user_reg
            );
            RAISE NOTICE 'Datos guardados con xito';

        WHEN 2 THEN
            -- Actualizacin
            IF EXISTS (SELECT 1 FROM servicios WHERE idServicio = codecode) THEN
                UPDATE servicios
                SET 
                    nombreServicio = nameserv,
                    categoria_servi = cat,
                    orden = orde,
                    precioBase = pb,
                    precioReal = pr,
                    observaciones = observ,
                    estado_servi = estad,  -- Actualizar el nuevo campo booleano
                    user_actualizacion = user_reg
                WHERE idServicio = codecode;
                RAISE NOTICE 'Actualizacin realizada correctamente';
            ELSE
                RAISE EXCEPTION 'No se encontr el servicio con ID %', codecode;
            END IF;

        WHEN 3 THEN
            -- Eliminacin
            IF EXISTS (SELECT 1 FROM servicios WHERE idServicio = codecode) THEN
                DELETE FROM servicios WHERE idServicio = codecode;
                RAISE NOTICE 'Eliminado correctamente';
            ELSE
                RAISE EXCEPTION 'No se encontr el servicio con ID % para eliminar', codecode;
            END IF;

        ELSE
            -- Operacin no vlida
            RAISE EXCEPTION 'Operacin no vlida: %', operacion;
    END CASE;
END;
$function$
;

-- Permissions

ALTER FUNCTION public.spfuncionservicios(int4, text, int4, numeric, numeric, numeric, int4, text, bool, text) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spfuncionservicios(int4, text, int4, numeric, numeric, numeric, int4, text, bool, text) TO huanchaco;

-- DROP FUNCTION public.spfuncionservicios(int4, text, int4, numeric, numeric, numeric, int4, text, text);

CREATE OR REPLACE FUNCTION public.spfuncionservicios(codecode integer, nameserv text, cat integer, orde numeric, pb numeric, pr numeric, operacion integer, observ text, user_reg text)
 RETURNS void
 LANGUAGE plpgsql
AS $function$
BEGIN
    CASE operacion 
        WHEN 1 THEN
           
            INSERT INTO servicios (
                nombreServicio,
                categoria_servi,
                orden, 
                precioBase, 
                precioReal, 
                observaciones,                            
                user_registro
            ) VALUES (
                nameserv, 
                cat,
                orde,  
                pb, 
                pr, 
                observ, 
                user_reg
            );
            RAISE NOTICE 'Datos guardados con xito';

        WHEN 2 THEN
            
            IF EXISTS (SELECT 1 FROM servicios WHERE idServicio = codecode) THEN
                UPDATE servicios
                SET 
                    nombreServicio = nameserv,
                    categoria_servi = cat,
                    orden = orde,
                    precioBase = pb,
                    precioReal = pr,
                    observaciones = observ,
                    user_actualizacion = user_reg
                WHERE idServicio = codecode;
                RAISE NOTICE 'Actualizacin realizada correctamente';
            ELSE
                RAISE EXCEPTION 'No se encontr el servicio con ID %', codecode;
            END IF;

        WHEN 3 THEN
            
            IF EXISTS (SELECT 1 FROM servicios WHERE idServicio = codecode) THEN
                DELETE FROM servicios WHERE idServicio = codecode;
                RAISE NOTICE 'Eliminado correctamente';
            ELSE
                RAISE EXCEPTION 'No se encontr el servicio con ID % para eliminar', codecode;
            END IF;

        ELSE
            
            RAISE EXCEPTION 'Operacin no vlida: %', operacion;
    END CASE;
END;
$function$
;

-- Permissions

ALTER FUNCTION public.spfuncionservicios(int4, text, int4, numeric, numeric, numeric, int4, text, text) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.spfuncionservicios(int4, text, int4, numeric, numeric, numeric, int4, text, text) TO huanchaco;

-- DROP FUNCTION public.splistadonombres(int4);

CREATE OR REPLACE FUNCTION public.splistadonombres(codigo integer)
 RETURNS text
 LANGUAGE plpgsql
AS $function$
declare nambre text;

begin

nambre=(select array_agg(s.nombreservicio) from historialClienteProtocolo as hcp left join protocoloServicio as ps
on hcp.idprotocoloservicio=ps.idprotocoloservicio left join servicios as s on ps.idservicio=s.idservicio
where hcp.n_orden=codigo limit 1);
RETURN nambre;

end
$function$
;

-- Permissions

ALTER FUNCTION public.splistadonombres(int4) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.splistadonombres(int4) TO huanchaco;

-- DROP FUNCTION public.sppreciomarsa(int4);

CREATE OR REPLACE FUNCTION public.sppreciomarsa(codigo integer)
 RETURNS integer
 LANGUAGE plpgsql
AS $function$
declare mensaje integer;
declare valor1 numeric(10,2);
declare valor2 numeric(10,2);
declare valor3 numeric(10,2);
declare valor5 numeric(10,2);
declare valor4 text;
declare valor6 text;
declare a1 boolean;
declare a2 boolean;
declare a3 boolean;
declare a4 boolean;

begin
valor1=(select n_orden from n_orden_ocupacional where UPPER(nom_examen) LIKE '%CUANTITATIVA%' and n_orden=codigo);
valor2=(select n_orden from n_orden_ocupacional where tipoprueba like 'P1%' and n_orden=codigo);
valor5=(select n_orden from n_orden_ocupacional where tipoprueba like 'P2%' and n_orden=codigo);
valor4=(select reactivoono(codigo));
valor6=(select tipoprueba from n_orden_ocupacional where n_orden=codigo);
valor3=(select n_orden from n_orden_ocupacional where UPPER(nom_examen) LIKE '%COVID-19%' and n_orden=codigo);

mensaje=0;
if(valor1 IS NULL and valor3 is null  )
then
if(codigo=valor2 and valor4='NO REACTIVO')
then
mensaje=70;
end if;
if(codigo=valor2 and valor4='REACTIVO')
then
mensaje=105;
end if;

if(codigo=valor5)
then
mensaje=105;
end if;

if(valor6='PS')
then
mensaje=45;
end if;
end if;


RETURN mensaje;

end
$function$
;

-- Permissions

ALTER FUNCTION public.sppreciomarsa(int4) OWNER TO nautica;
GRANT ALL ON FUNCTION public.sppreciomarsa(int4) TO nautica;

