	razon_empresa text NOT NULL,
	direccion_empresa varchar(255) NULL,
	telefono_empresa varchar(50) NULL,
	responsable_empresa text NULL,
	email_empresa text NULL,
	api_token text NULL,
	CONSTRAINT empresas_pkey PRIMARY KEY (ruc_empresa),
	CONSTRAINT empresas_razon_empresa_key UNIQUE (razon_empresa)
);
CREATE INDEX razon_empresa ON empresas USING btree (razon_empresa);

