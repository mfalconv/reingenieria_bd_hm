	user_medico_ocup text NULL,
	CONSTRAINT oftalmologia_pkey PRIMARY KEY (cod_of),
	CONSTRAINT oftalmologia_n_orden_fkey FOREIGN KEY (n_orden) REFERENCES public.n_orden_ocupacional(n_orden)
);

-- Permissions

ALTER TABLE public.oftalmologia OWNER TO postgres;
GRANT ALL ON TABLE public.oftalmologia TO postgres;


