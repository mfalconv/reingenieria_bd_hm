-- DROP FUNCTION public.actualizarauditoriasellosmedicosyespecialistas(text, int4, text, text);

CREATE OR REPLACE FUNCTION public.actualizarauditoriasellosmedicosyespecialistas(condicion_enfermedad text, historia_c integer, user_medico text, user_especialista text)
 RETURNS text
 LANGUAGE plpgsql
AS $function$

	DECLARE usuario_medico text;
	DECLARE usuario_especialista text;
	DECLARE user_dnis bigint;

BEGIN

if(condicion_enfermedad='antece_enfermedades_altura') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update antece_enfermedades_altura set dni_user=user_dnis, user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='b_uso_respiradores') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update b_uso_respiradores set dni_user=user_dnis, user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='historia_oc_info') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update historia_oc_info set dni_user=user_dnis, user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='antecedentes_patologicos') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update antecedentes_patologicos set dni_user=user_dnis, user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='cuestionario_nordico') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update cuestionario_nordico set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='evaluacion_musculo_esqueletica') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update evaluacion_musculo_esqueletica set dni_user=user_dnis, user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='certificado_manipuladores_barrick') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update certificado_manipuladores_barrick set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='analisis_bioquimicos') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select USUARIO_USER into usuario_especialista from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_especialista;


select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update analisis_bioquimicos set  user_registro=usuario_medico,user_medico_ocup=usuario_especialista where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='lab_clinico') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select USUARIO_USER into usuario_especialista from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_especialista;


select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update lab_clinico set  user_registro=usuario_medico,user_medico_ocup=usuario_especialista where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='anexo_agroindustrial') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;


select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update anexo_agroindustrial set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='anexo7c') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update anexo7c set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='certificado_aptitud_medico_ocupacional') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update certificado_aptitud_medico_ocupacional set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='oit') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update oit set  user_registro=usuario_medico,dni_user=user_dnis where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='informe_electrocardiograma') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update informe_electrocardiograma set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='ficha_audiologica') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select USUARIO_USER into usuario_especialista from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_especialista;


select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update ficha_audiologica set  user_registro=usuario_medico,user_medico_ocup=usuario_especialista, txtresponsable=user_medico,
	txtmedico=user_especialista where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='cuestionario_audiometria') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update cuestionario_audiometria set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='odontograma') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update odontograma set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='psicologiafobias') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update psicologiafobias set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='informe_psicologico') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update informe_psicologico set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='evaluacion_psicologica') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update evaluacion_psicologica set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='oftalmologia') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select USUARIO_USER into usuario_especialista from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_especialista;


select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update oftalmologia set  user_registro=usuario_medico,user_medico_ocup=usuario_especialista where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='oftalmologia_lo') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select USUARIO_USER into usuario_especialista from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_especialista;


select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update oftalmologia_lo set  user_registro=usuario_medico,user_medico_ocup=usuario_especialista where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='consentimiento_dosaje') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update consentimiento_dosaje set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='aptitud_medico_ocupacional11') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update aptitud_medico_ocupacional11 set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='certificacion_medica_altura') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update certificacion_medica_altura set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='aptitud_trabajos_encaliente') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update aptitud_trabajos_encaliente set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='aptitud_licencia_conduciri') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update aptitud_licencia_conduciri set  user_registro=usuario_medico,nom_medico=user_medico where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='aptitud_altura_poderosa') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update aptitud_altura_poderosa set  user_registro=usuario_medico,nom_medico=user_medico where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='certificado_altura_poderosa') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update certificado_altura_poderosa set  user_registro=usuario_medico,dni_user=user_dnis where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='b_certificado_altura') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update b_certificado_altura set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='b_certificado_conduccion') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update b_certificado_conduccion set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='ficha_sas') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update ficha_sas set  user_registro=usuario_medico,dni_user=user_dnis where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='evaluacion_musculo_esqueletica2021') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select USUARIO_USER into usuario_especialista from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_especialista;


select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update evaluacion_musculo_esqueletica2021 set  user_registro=usuario_medico, txtmedico=user_especialista where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='audiometria_2023') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select USUARIO_USER into usuario_especialista from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_especialista;


select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update audiometria_2023 set  user_registro=usuario_medico,user_medico_ocup=usuario_especialista where n_orden=historia_c;
return 'OK';
end if;

if(condicion_enfermedad='oftalmologia2021') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select USUARIO_USER into usuario_especialista from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_especialista;


select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update oftalmologia2021 set  user_registro=usuario_medico,user_medico_ocup=usuario_especialista where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='test_fatiga_somnolencia') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select USUARIO_USER into usuario_especialista from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_especialista;


select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update test_fatiga_somnolencia set  user_registro=usuario_medico,dni_user=user_dnis, txtmedico=user_medico where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='anexo16a') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select USUARIO_USER into usuario_especialista from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_especialista;


select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update anexo16a set  user_registro=usuario_medico,dni_user=user_dnis where n_orden=historia_c;
return 'OK';
end if;


if(condicion_enfermedad='radiografia_torax') then

select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
select USUARIO_USER into usuario_especialista from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_especialista;


select DNI_USER into user_dnis from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;
update radiografia_torax set  user_registro=usuario_medico where n_orden=historia_c;
return 'OK';
end if;


END;
$function$
;

-- Permissions

ALTER FUNCTION public.actualizarauditoriasellosmedicosyespecialistas(text, int4, text, text) OWNER TO pierola;
GRANT ALL ON FUNCTION public.actualizarauditoriasellosmedicosyespecialistas(text, int4, text, text) TO pierola;

-- DROP FUNCTION public.actualizarfirmassellosmedicos(int4, text);

CREATE OR REPLACE FUNCTION public.actualizarfirmassellosmedicos(historia_c integer, user_medico text)
 RETURNS text
 LANGUAGE plpgsql
AS $function$
	DECLARE validar_existenia_toxicologia BIGINT;
        DECLARE validar_existenia_lgonadotropina BIGINT;
	DECLARE validar_existenia_ac_bioquimica2022 BIGINT;
	DECLARE validar_existenia_perfil_hepatico BIGINT;
	DECLARE validar_existenia_microbiologia BIGINT;
	DECLARE validar_existenia_lhepatitis BIGINT;
	DECLARE validar_existenia_inmunologia BIGINT;
	DECLARE validar_existenia_lab_clinico BIGINT;
	DECLARE validar_existenia_analisis_bioquimicos BIGINT;
	DECLARE validar_existenia_audiometria_2023 BIGINT;
	DECLARE validar_existenia_oftalmologia2021 BIGINT;
	DECLARE validar_existenia_oftalmologia_lo BIGINT;
	DECLARE validar_existenia_oftalmologia BIGINT;
	DECLARE validar_existenia_ficha_audiologica BIGINT;
	DECLARE validar_existenia_audiometria_po BIGINT;
	DECLARE validar_existenia_antecedentes_patologicos BIGINT;
	DECLARE validar_existenia_historia_oc_info BIGINT;
	DECLARE validar_existenia_certificado_altura_poderosa BIGINT;
	DECLARE validar_existenia_evaluacion_musculo_esqueletica BIGINT;
	DECLARE validar_existenia_certificado_aptitud_medico_ocupacional BIGINT;
	DECLARE validar_existenia_antece_enfermedades_altura BIGINT;
	DECLARE validar_existenia_aptitud_altura_poderosa BIGINT;
	DECLARE validar_existenia_anexo7c BIGINT;
	DECLARE validar_existenia_cuestionario_nordico BIGINT;
	DECLARE validar_existenia_anexo16a BIGINT;
	DECLARE validar_existenia_b_uso_respiradores BIGINT;
	DECLARE validar_existenia_test_fatiga_somnolencia BIGINT;
	DECLARE validar_existenia_b_certificado_conduccion BIGINT;
	DECLARE validar_existenia_ficha_sas BIGINT;
	DECLARE validar_existenia_evaluacion_musculo_esqueletica2021 BIGINT;
	DECLARE validar_existenia_b_certificado_altura BIGINT;
	DECLARE validar_existenia_informe_electrocardiograma BIGINT;
	DECLARE validar_existenia_certificado_aptitud_medico_resumen BIGINT;

	
	DECLARE usuario_medico text;


BEGIN
select USUARIO_USER into usuario_medico from usuarios WHERE TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))=user_medico;

select count(n_orden) into validar_existenia_toxicologia from toxicologia where n_orden=historia_c;
select count(n_orden) into validar_existenia_lgonadotropina from lgonadotropina where n_orden=historia_c;
select count(n_orden) into validar_existenia_ac_bioquimica2022 from ac_bioquimica2022 where n_orden=historia_c;
select count(n_orden) into validar_existenia_perfil_hepatico from perfil_hepatico where n_orden=historia_c;
select count(n_orden) into validar_existenia_microbiologia from microbiologia where n_orden=historia_c;
select count(n_orden) into validar_existenia_lhepatitis from lhepatitis where n_orden=historia_c;
select count(n_orden) into validar_existenia_inmunologia from inmunologia where n_orden=historia_c;
select count(n_orden) into validar_existenia_lab_clinico from lab_clinico where n_orden=historia_c;
select count(n_orden) into validar_existenia_analisis_bioquimicos from analisis_bioquimicos where n_orden=historia_c;
select count(n_orden) into validar_existenia_audiometria_2023 from audiometria_2023 where n_orden=historia_c;
select count(n_orden) into validar_existenia_oftalmologia2021 from oftalmologia2021 where n_orden=historia_c;
select count(n_orden) into validar_existenia_oftalmologia_lo from oftalmologia_lo where n_orden=historia_c;
select count(n_orden) into validar_existenia_oftalmologia from oftalmologia where n_orden=historia_c;
select count(n_orden) into validar_existenia_ficha_audiologica from ficha_audiologica where n_orden=historia_c;
select count(n_orden) into validar_existenia_audiometria_po from audiometria_po where n_orden=historia_c;
select count(n_orden) into validar_existenia_antecedentes_patologicos from antecedentes_patologicos where n_orden=historia_c;
select count(n_orden) into validar_existenia_historia_oc_info from historia_oc_info where n_orden=historia_c;
select count(n_orden) into validar_existenia_certificado_altura_poderosa from certificado_altura_poderosa where n_orden=historia_c;
select count(n_orden) into validar_existenia_evaluacion_musculo_esqueletica from evaluacion_musculo_esqueletica where n_orden=historia_c;
select count(n_orden) into validar_existenia_certificado_aptitud_medico_ocupacional from certificado_aptitud_medico_ocupacional where n_orden=historia_c;
select count(n_orden) into validar_existenia_antece_enfermedades_altura from antece_enfermedades_altura where n_orden=historia_c;
select count(n_orden) into validar_existenia_aptitud_altura_poderosa from aptitud_altura_poderosa where n_orden=historia_c;
select count(n_orden) into validar_existenia_anexo7c from anexo7c where n_orden=historia_c;
select count(n_orden) into validar_existenia_cuestionario_nordico from cuestionario_nordico where n_orden=historia_c;
select count(n_orden) into validar_existenia_anexo16a from anexo16a where n_orden=historia_c;
select count(n_orden) into validar_existenia_b_uso_respiradores from b_uso_respiradores where n_orden=historia_c;
select count(n_orden) into validar_existenia_test_fatiga_somnolencia from test_fatiga_somnolencia where n_orden=historia_c;
select count(n_orden) into validar_existenia_b_certificado_conduccion from b_certificado_conduccion where n_orden=historia_c;
select count(n_orden) into validar_existenia_ficha_sas from ficha_sas where n_orden=historia_c;
select count(n_orden) into validar_existenia_evaluacion_musculo_esqueletica2021 from evaluacion_musculo_esqueletica2021 where n_orden=historia_c;
select count(n_orden) into validar_existenia_b_certificado_altura from b_certificado_altura where n_orden=historia_c;
select count(n_orden) into validar_existenia_informe_electrocardiograma from informe_electrocardiograma where n_orden=historia_c;
select count(n_orden) into validar_existenia_certificado_aptitud_medico_resumen from certificado_aptitud_medico_resumen where n_orden=historia_c;


--
IF(validar_existenia_antecedentes_patologicos>0) then
update antecedentes_patologicos set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_historia_oc_info>0) then
update historia_oc_info set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_certificado_altura_poderosa>0) then
update certificado_altura_poderosa set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_evaluacion_musculo_esqueletica>0) then
update evaluacion_musculo_esqueletica set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_certificado_aptitud_medico_ocupacional>0) then
update certificado_aptitud_medico_ocupacional set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_antece_enfermedades_altura>0) then
update antece_enfermedades_altura set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_aptitud_altura_poderosa>0) then
update aptitud_altura_poderosa set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_anexo7c>0) then
update anexo7c set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_cuestionario_nordico>0) then
update cuestionario_nordico set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_anexo16a>0) then
update anexo16a set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_b_uso_respiradores>0) then
update b_uso_respiradores set user_registro=usuario_medico where n_orden=historia_c;
end if;


IF(validar_existenia_test_fatiga_somnolencia>0) then
update test_fatiga_somnolencia set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_b_certificado_conduccion>0) then
update b_certificado_conduccion set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_ficha_sas>0) then
update ficha_sas set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_evaluacion_musculo_esqueletica2021>0) then
update evaluacion_musculo_esqueletica2021 set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_b_certificado_altura>0) then
update b_certificado_altura set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_informe_electrocardiograma>0) then
update informe_electrocardiograma set user_registro=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_certificado_aptitud_medico_resumen>0) then
update certificado_aptitud_medico_resumen set user_registro=usuario_medico where n_orden=historia_c;
end if;

--

IF(validar_existenia_toxicologia>0)then
update toxicologia set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_lgonadotropina>0)then
update lgonadotropina set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_ac_bioquimica2022>0)then
update ac_bioquimica2022 set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_perfil_hepatico>0)then
update perfil_hepatico set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_microbiologia>0)then
update microbiologia set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_lhepatitis>0)then
update lhepatitis set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_inmunologia>0)then
update inmunologia set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_lab_clinico>0)then
update lab_clinico set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_analisis_bioquimicos>0)then
update analisis_bioquimicos set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_audiometria_2023>0)then
update audiometria_2023 set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_oftalmologia2021>0)then
update oftalmologia2021 set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_oftalmologia_lo>0)then
update oftalmologia_lo set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_oftalmologia>0)then
update oftalmologia set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_ficha_audiologica>0)then
update ficha_audiologica set user_medico_ocup=usuario_medico where n_orden=historia_c;
end if;

IF(validar_existenia_audiometria_po>0) then
update audiometria_po set user_registro=usuario_medico where n_orden=historia_c;

end if;


return 'OK';
END;
$function$
;

-- Permissions

ALTER FUNCTION public.actualizarfirmassellosmedicos(int4, text) OWNER TO pierola;
GRANT ALL ON FUNCTION public.actualizarfirmassellosmedicos(int4, text) TO pierola;

-- DROP FUNCTION public.actualizarnombredoctoraportablaestaticafirma(text, int4, int4);

CREATE OR REPLACE FUNCTION public.actualizarnombredoctoraportablaestaticafirma(opcion_param text, historia_c integer, dni_param integer)
 RETURNS text
 LANGUAGE plpgsql
AS $function$

	DECLARE usuario_medico text;

BEGIN

if(opcion_param='certificado_aptitud_medico_ocupacional') then

select (TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))) into usuario_medico from usuarios WHERE dni_user=dni_param;
update certificado_aptitud_medico_ocupacional set nom_medico=usuario_medico  where n_orden=historia_c;
return 'OK';
end if;

if(opcion_param='aptitud_medico_ocupacional11') then

select (TRIM(UPPER(APELLIDO_USER))||' '||TRIM(UPPER(NOMBRE_USER))) into usuario_medico from usuarios WHERE dni_user=dni_param;
update aptitud_medico_ocupacional11 set nom_medico=usuario_medico  where n_orden=historia_c;
return 'OK';
end if;


END;
$function$
;

-- Permissions

ALTER FUNCTION public.actualizarnombredoctoraportablaestaticafirma(text, int4, int4) OWNER TO pierola;
GRANT ALL ON FUNCTION public.actualizarnombredoctoraportablaestaticafirma(text, int4, int4) TO pierola;

-- DROP FUNCTION public.backend_registro_historiaclinica_ocupacional(int8, int8, int8, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, bool, bool, bool, varchar, varchar, varchar, bool, bool, bool, bool, bool, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, bool, bool, bool, bool, bool, bool);

CREATE OR REPLACE FUNCTION public.backend_registro_historiaclinica_ocupacional(tipo_operacion_exp bigint, n_orden_exp bigint, cod_pa_exp bigint, razon_empresa_exp character varying, razon_contrata_exp character varying, nom_ex_exp character varying, altura_po_exp character varying, mineral_po_exp character varying, fecha_apertura_po_exp character varying, precio_po_exp character varying, estado_ex_exp character varying, nom_examen_exp character varying, cargo_de_exp character varying, area_o_exp character varying, n_medico_exp character varying, n_hora_exp character varying, tipo_pago_exp character varying, n_fisttest_exp boolean, n_psicosen_exp boolean, n_testaltura_exp boolean, grupo_san_exp character varying, grupo_factor_san_exp character varying, cod_clinica_exp character varying, visual_compl_exp boolean, trab_calientes_exp boolean, chk_covid1_exp boolean, chk_covid2_exp boolean, manip_alimentos_exp boolean, txtobserv1_exp character varying, txtobserv2_exp character varying, cod_sede_exp character varying, tipo_prueba_covid_exp character varying, tipo_prueba_exp character varying, nombre_hotel_exp character varying, protocolo_exp character varying, precio_adic_exp character varying, autoriza_exp character varying, n_operacion_exp character varying, herra_manuales_exp boolean, rxc_dorso_lumbar_exp boolean, rxc_lumbar_exp boolean, rxc_lumbosacra_exp boolean, rxc_plomos_exp boolean, mercurioo_exp boolean)
 RETURNS TABLE(id_resp bigint, mensaje text)
 LANGUAGE plpgsql
AS $function$
	DECLARE codigo_sede_param integer;
	DECLARE n_orden_hm integer;


BEGIN
			Select id into codigo_sede_param from sede_multisucursal where codigo_sucursal=cod_sede_exp;

			if(tipo_operacion_exp=2) then
		update n_orden_ocupacional set cod_pa=cod_pa_exp,razon_empresa=razon_empresa_exp,razon_contrata=razon_contrata_exp,nom_ex=nom_ex_exp,
		altura_po=altura_po_exp,mineral_po=mineral_po_exp,fecha_apertura_po=TO_DATE(fecha_apertura_po_exp,'YYYY-MM-DD'),precio_po=CAST(precio_po_exp as money),
		estado_ex=estado_ex_exp,nom_examen=nom_examen_exp,cargo_de=cargo_de_exp,area_o=area_o_exp,n_medico=n_medico_exp,n_hora=CAST (n_hora_exp as time without time zone),
		tipo_pago=tipo_pago_exp,n_fisttest=n_fisttest_exp,n_psicosen=n_psicosen_exp,n_testaltura=n_testaltura_exp,color=49,gruposan=grupo_san_exp,grupofactorsan=grupo_factor_san_exp,
		cod_clinica=cod_clinica_exp,visual_compl=visual_compl_exp,trab_calientes=trab_calientes_exp,chkcovid1=chk_covid1_exp,chkcovid2=chk_covid2_exp,manip_alimentos=manip_alimentos_exp,
		txtobserv1=txtobserv1_exp,txtobserv2=txtobserv2_exp,cod_sede=codigo_sede_param,tipo_prueba_covid=tipo_prueba_covid_exp,tipoPrueba=tipo_prueba_exp,nombrehotel=nombre_hotel_exp,
		protocolo=protocolo_exp,precio_adic=CAST(precio_adic_exp AS money),autoriza=autoriza_exp,n_operacion=n_operacion_exp, herra_manuales=herra_manuales_exp, rxc_dorso_lumbar=rxc_dorso_lumbar_exp,
		 rxc_lumbar=rxc_lumbar_exp, rxc_lumbosacra=rxc_lumbosacra_exp, rxc_plomos=rxc_plomos_exp, mercurioo=mercurioo_exp where n_orden=n_orden_exp;
		RETURN QUERY 
		select CAST(1 AS bigint) as id_resp, CAST('El usuario se actualizo con exito!' AS TEXT) as mensaje;

		else
		
		INSERT INTO n_orden_ocupacional(cod_pa, razon_empresa, razon_contrata,
                        nom_ex, altura_po,mineral_po, fecha_apertura_po,precio_po,estado_ex,nom_examen,cargo_de,area_o,n_medico,n_hora,
                        tipo_pago,n_fisttest,n_psicosen,n_testaltura,color,gruposan,grupofactorsan,visual_compl,
                        trab_calientes,chkcovid1,chkcovid2,manip_alimentos, txtobserv1,txtobserv2,cod_sede,tipo_prueba_covid,tipoPrueba,nombrehotel,protocolo,
                        precio_adic,autoriza,n_operacion, herra_manuales, rxc_dorso_lumbar, rxc_lumbar, rxc_lumbosacra, rxc_plomos,mercurioo)
		values (cod_pa_exp, razon_empresa_exp, razon_contrata_exp,nom_ex_exp, altura_po_exp, mineral_po_exp, TO_DATE(fecha_apertura_po_exp,'YYYY-MM-DD'), CAST(precio_po_exp as money), estado_ex_exp,
	nom_examen_exp, cargo_de_exp, area_o_exp,n_medico_exp, CAST (n_hora_exp as time without time zone), tipo_pago_exp,n_fisttest_exp, n_psicosen_exp,n_testaltura_exp, 49, grupo_san_exp,grupo_factor_san_exp,
	visual_compl_exp, trab_calientes_exp,chk_covid1_exp,chk_covid2_exp,manip_alimentos_exp,txtobserv1_exp,txtobserv2_exp,codigo_sede_param,tipo_prueba_covid_exp ,tipo_prueba_exp,nombre_hotel_exp,
	protocolo_exp,CAST(precio_adic_exp AS money),autoriza_exp,n_operacion_exp,herra_manuales_exp, rxc_dorso_lumbar_exp, rxc_lumbar_exp,rxc_lumbosacra_exp ,rxc_plomos_exp, mercurioo_exp  ) RETURNING n_orden into n_orden_hm;
                        
		RETURN QUERY 
		select CAST(n_orden_hm as bigint) as id_resp, CAST('El usuario se registro con exito!' AS TEXT) as mensaje;
		end if;

END; $function$
;

-- Permissions

ALTER FUNCTION public.backend_registro_historiaclinica_ocupacional(int8, int8, int8, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, bool, bool, bool, varchar, varchar, varchar, bool, bool, bool, bool, bool, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, bool, bool, bool, bool, bool, bool) OWNER TO pierola;
GRANT ALL ON FUNCTION public.backend_registro_historiaclinica_ocupacional(int8, int8, int8, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, bool, bool, bool, varchar, varchar, varchar, bool, bool, bool, bool, bool, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, bool, bool, bool, bool, bool, bool) TO pierola;

-- DROP FUNCTION public.detalle_archivo_servidor(int8, int8);

CREATE OR REPLACE FUNCTION public.detalle_archivo_servidor(p_hc bigint, p_ta bigint)
 RETURNS TABLE(id_archivos integer, estado boolean, fecha_actualizacion date, fecha_registro date, historia_clinica integer, nombre_archivo text, ruta_archivo text, servidor text, user_actualizacion text, user_registro text, id_tipo_archivo integer)
 LANGUAGE plpgsql
AS $function$
BEGIN
    RETURN QUERY 
	select *from archivos_servidores as arcs where arcs.historia_clinica=p_hc and arcs.id_tipo_archivo=p_ta;

END; $function$
;

-- Permissions

ALTER FUNCTION public.detalle_archivo_servidor(int8, int8) OWNER TO pierola;
GRANT ALL ON FUNCTION public.detalle_archivo_servidor(int8, int8) TO pierola;

-- DROP FUNCTION public.ejemplo(varchar);

CREATE OR REPLACE FUNCTION public.ejemplo(filtro character varying)
 RETURNS TABLE(n_orden text, razon_contrata text)
 LANGUAGE plpgsql
AS $function$
BEGIN
 RETURN QUERY SELECT
 n_orden,
 razon_contrata
 FROM
 n_orden_ocupacional
 WHERE
 razon_empresa ILIKE filtro ;
END; $function$
;

-- Permissions

ALTER FUNCTION public.ejemplo(varchar) OWNER TO postgres;
GRANT ALL ON FUNCTION public.ejemplo(varchar) TO postgres;

-- DROP FUNCTION public.listado_datos_pacientes_por_usuario(int4);

CREATE OR REPLACE FUNCTION public.listado_datos_pacientes_por_usuario(p_id_user integer)
 RETURNS TABLE(dni integer, apellidos text, nombres text, fecha_examen date, id_entidad bigint)
 LANGUAGE plpgsql
AS $function$
BEGIN
    RETURN QUERY 
    select DISTINCT(tb.cod_pa) as dni, tb.apellidos_pa as apellidos,tb.nombres_pa as nombres,tb.fecha_apertura_po as fecha_examen, row_number() over() as id_entidad  from(
	select dp.cod_pa,dp.apellidos_pa,dp.nombres_pa, noo.fecha_apertura_po from datos_paciente as dp inner join n_orden_ocupacional as noo on dp.cod_pa=noo.cod_pa
		where noo.razon_empresa in (select razon_empresa from empresas as em where em.ruc_empresa in(select ruc from empresa_contrata_web where id_user=p_id_user))
	UNION
	select dp.cod_pa,dp.apellidos_pa,dp.nombres_pa, noo.fecha_apertura_po from datos_paciente as dp inner join n_orden_ocupacional as noo on dp.cod_pa=noo.cod_pa
	where noo.razon_empresa in (select razon_contrata from contratas as em where em.ruc_contrata in(select ruc from empresa_contrata_web where id_user=p_id_user))
	) tb order by tb.fecha_apertura_po desc;
END; $function$
;

-- Permissions

ALTER FUNCTION public.listado_datos_pacientes_por_usuario(int4) OWNER TO pierola;
GRANT ALL ON FUNCTION public.listado_datos_pacientes_por_usuario(int4) TO pierola;

-- DROP FUNCTION public.listado_datos_pacientes_por_usuario(int8);

CREATE OR REPLACE FUNCTION public.listado_datos_pacientes_por_usuario(p_id_user bigint)
 RETURNS TABLE(dni integer, apellidos text, nombres text, fecha_examen date, id_historial_paciente bigint)
 LANGUAGE plpgsql
AS $function$
BEGIN
    RETURN QUERY 
    select DISTINCT(tb.cod_pa) as dni, tb.apellidos_pa as apellidos,tb.nombres_pa as nombres,tb.fecha_apertura_po as fecha_examen, row_number() over() as id_historial_paciente  from(
	select dp.cod_pa,dp.apellidos_pa,dp.nombres_pa, noo.fecha_apertura_po from datos_paciente as dp inner join n_orden_ocupacional as noo on dp.cod_pa=noo.cod_pa
		where noo.razon_empresa in (select razon_empresa from empresas as em where em.ruc_empresa in(select ruc from empresa_contrata_web where id_user=p_id_user))
	UNION
	select dp.cod_pa,dp.apellidos_pa,dp.nombres_pa, noo.fecha_apertura_po from datos_paciente as dp inner join n_orden_ocupacional as noo on dp.cod_pa=noo.cod_pa
	where noo.razon_empresa in (select razon_contrata from contratas as em where em.ruc_contrata in(select ruc from empresa_contrata_web where id_user=p_id_user))
	) tb order by tb.fecha_apertura_po desc;
END; $function$
;

-- Permissions

ALTER FUNCTION public.listado_datos_pacientes_por_usuario(int8) OWNER TO pierola;
GRANT ALL ON FUNCTION public.listado_datos_pacientes_por_usuario(int8) TO pierola;

-- DROP FUNCTION public.listado_detalle_historia_clinica(int8);

CREATE OR REPLACE FUNCTION public.listado_detalle_historia_clinica(p_dni bigint)
 RETURNS TABLE(historia_clinica integer, empresa text, contrata text, fecha_apertura date, examen text, estado text, cargo text, area text, grupo_sanguineo text)
 LANGUAGE plpgsql
AS $function$
BEGIN
    RETURN QUERY 
	select n_orden as historia_clinica,razon_empresa as empresa, razon_contrata as contrata, fecha_apertura_po as fecha_apertura, nom_examen as examen, estado_ex as estado, 
	cargo_de as cargo, area_o as area,grupofactorsan as grupo_sanguineo  from n_orden_ocupacional where cod_pa=p_dni;

END; $function$
;

-- Permissions

ALTER FUNCTION public.listado_detalle_historia_clinica(int8) OWNER TO pierola;
GRANT ALL ON FUNCTION public.listado_detalle_historia_clinica(int8) TO pierola;

-- DROP FUNCTION public.listado_empresa_contrata();

CREATE OR REPLACE FUNCTION public.listado_empresa_contrata()
 RETURNS TABLE(razon_social text, ruc text, id_entidad bigint)
 LANGUAGE plpgsql
AS $function$
BEGIN
    RETURN QUERY select DISTINCT(tb.razon_social),tb.ruc, row_number() over() as id_entidad from (
	select  razon_empresa as razon_social,ruc_empresa as ruc from empresas
	union
	select razon_contrata as razon_social,ruc_contrata as ruc from contratas) tb order by id_entidad;
END; $function$
;

-- Permissions

ALTER FUNCTION public.listado_empresa_contrata() OWNER TO pierola;
GRANT ALL ON FUNCTION public.listado_empresa_contrata() TO pierola;

-- DROP FUNCTION public.listadodetallehistorialusuarios(varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar);

CREATE OR REPLACE FUNCTION public.listadodetallehistorialusuarios(user_name character varying, fecha_inicio character varying, fecha_fin character varying, tipo character varying, ruc_contrata_user character varying, ruc_empresa_user character varying, sede_user character varying, dni_user character varying)
 RETURNS TABLE(id bigint, historia_clinica text, orden integer, empresa text, contrata text, fecha_examen date, examen text, estado text, cargo text, area text, grupo_sanguineo text)
 LANGUAGE plpgsql
AS $function$
	DECLARE clave INTEGER;

BEGIN
	select sm.id into  clave from sede_multisucursal AS sm where sm.codigo_sucursal=sede_user;

	if(sede_user='HMAC') then

	if(tipo='EMPRESA' AND ruc_empresa_user='20477167561') then
		RETURN QUERY 
		 select  row_number() over() as id,  CONCAT( sm.codigo_sucursal,'-',noo.referencia) as historia_clinica,CAST(noo.referencia AS integer) as orden, noo.razon_empresa as empresa, 
			noo.razon_contrata as contrata, noo.fecha_apertura_po as fecha_examen,
			noo.nom_examen as examen, noo.estado_ex as examen, noo.cargo_de as cargo,noo.area_o as area,noo.gruposan AS grupo_sanguineo 
			from n_orden_ocupacional as noo inner join datos_paciente as dp on dp.cod_pa= noo.cod_pa inner join sede_multisucursal as sm on
			noo.cod_sede=sm.id
			where dp.cod_pa= CAST(dni_user as integer) and sm.id=clave
			order by noo.fecha_apertura_po desc;
	end if;		

	if(tipo='EMPRESA' AND ruc_empresa_user<>'20477167561') then
		RETURN QUERY 
		 select  row_number() over() as id,  CONCAT( sm.codigo_sucursal,'-',noo.referencia) as historia_clinica,CAST(noo.referencia AS integer) as orden, noo.razon_empresa as empresa, 
			noo.razon_contrata as contrata, noo.fecha_apertura_po as fecha_examen,
			noo.nom_examen as examen, noo.estado_ex as examen, noo.cargo_de as cargo,noo.area_o as area,noo.gruposan AS grupo_sanguineo
			from n_orden_ocupacional as noo inner join datos_paciente as dp on dp.cod_pa= noo.cod_pa inner join sede_multisucursal as sm on
			noo.cod_sede=sm.id inner join empresas as emp on emp.razon_empresa=noo.razon_empresa
			where dp.cod_pa= CAST(dni_user as integer) and sm.id=clave 
			and emp.ruc_empresa=ruc_empresa_user
			order by noo.fecha_apertura_po desc;

	end if;

		if(tipo='CONTRATA') then
		RETURN QUERY 
		 select  row_number() over() as id,  CONCAT( sm.codigo_sucursal,'-',noo.referencia) as historia_clinica,CAST(noo.referencia AS integer) as orden, noo.razon_empresa as empresa, 
			noo.razon_contrata as contrata, noo.fecha_apertura_po as fecha_examen,
			noo.nom_examen as examen, noo.estado_ex as examen, noo.cargo_de as cargo,noo.area_o as area,noo.gruposan AS grupo_sanguineo
			from n_orden_ocupacional as noo inner join datos_paciente as dp on dp.cod_pa= noo.cod_pa inner join sede_multisucursal as sm on
			noo.cod_sede=sm.id inner join contratas as cont on cont.razon_contrata=noo.razon_contrata
			where dp.cod_pa= CAST(dni_user as integer) and sm.id=clave
			and cont.ruc_contrata=ruc_contrata_user
			order by noo.fecha_apertura_po desc;
	

		end if;



	else
	
	if(tipo='EMPRESA' AND ruc_empresa_user='20477167561') then
		RETURN QUERY 
		 select  row_number() over() as id,  CONCAT( sm.codigo_sucursal,'-',noo.n_orden) as historia_clinica,noo.n_orden as orden, noo.razon_empresa as empresa, 
			noo.razon_contrata as contrata, noo.fecha_apertura_po as fecha_examen,
			noo.nom_examen as examen, noo.estado_ex as examen, noo.cargo_de as cargo,noo.area_o as area,(CASE WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden) >0 
	THEN (
		CONCAT((CASE WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.chko=true)>0 THEN 'O' 
		     WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.chka=true)>0 THEN 'A' 
		     WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.chkb=true)>0 THEN 'B' 
		     WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.chkab=true)>0 THEN 'AB' END),
			(CASE WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.rbrhpositivo=true)>0 THEN '+' 
			WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.rbrhnegativo=true)>0 THEN '-' END )
			)
		) 

		ELSE ( CASE WHEN (select nnn.gruposan from n_orden_ocupacional as nnn where nnn.n_orden=noo.n_orden) is null or 
			(select nnn.gruposan from n_orden_ocupacional as nnn where nnn.n_orden=noo.n_orden)='null' THEN 'N/A' 
			ELSE (select nnn.gruposan from n_orden_ocupacional as nnn where nnn.n_orden=noo.n_orden)  END) 

		END) AS grupo_sanguineo 
			from n_orden_ocupacional as noo inner join datos_paciente as dp on dp.cod_pa= noo.cod_pa inner join sede_multisucursal as sm on
			noo.cod_sede=sm.id
			where dp.cod_pa= CAST(dni_user as integer) and sm.id=clave
			order by noo.fecha_apertura_po desc;
	end if;		

	if(tipo='EMPRESA' AND ruc_empresa_user<>'20477167561') then
		RETURN QUERY 
		 select  row_number() over() as id,  CONCAT( sm.codigo_sucursal,'-',noo.n_orden) as historia_clinica,noo.n_orden as orden, noo.razon_empresa as empresa, 
			noo.razon_contrata as contrata, noo.fecha_apertura_po as fecha_examen,
			noo.nom_examen as examen, noo.estado_ex as examen, noo.cargo_de as cargo,noo.area_o as area,(CASE WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden) >0 
	THEN (
		CONCAT((CASE WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.chko=true)>0 THEN 'O' 
		     WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.chka=true)>0 THEN 'A' 
		     WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.chkb=true)>0 THEN 'B' 
		     WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.chkab=true)>0 THEN 'AB' END),
			(CASE WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.rbrhpositivo=true)>0 THEN '+' 
			WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.rbrhnegativo=true)>0 THEN '-' END )
			)
		) 

ELSE ( CASE WHEN (select nnn.gruposan from n_orden_ocupacional as nnn where nnn.n_orden=noo.n_orden) is null or 
	(select nnn.gruposan from n_orden_ocupacional as nnn where nnn.n_orden=noo.n_orden)='null' THEN 'N/A' 
		ELSE (select nnn.gruposan from n_orden_ocupacional as nnn where nnn.n_orden=noo.n_orden)  END) 

END) AS grupo_sanguineo
			from n_orden_ocupacional as noo inner join datos_paciente as dp on dp.cod_pa= noo.cod_pa inner join sede_multisucursal as sm on
			noo.cod_sede=sm.id inner join empresas as emp on emp.razon_empresa=noo.razon_empresa
			where dp.cod_pa= CAST(dni_user as integer) and sm.id=clave 
			and emp.ruc_empresa=ruc_empresa_user
			order by noo.fecha_apertura_po desc;

	end if;

		if(tipo='CONTRATA') then
		RETURN QUERY 
		 select  row_number() over() as id,  CONCAT( sm.codigo_sucursal,'-',noo.n_orden) as historia_clinica,noo.n_orden as orden, noo.razon_empresa as empresa, 
			noo.razon_contrata as contrata, noo.fecha_apertura_po as fecha_examen,
			noo.nom_examen as examen, noo.estado_ex as examen, noo.cargo_de as cargo,noo.area_o as area,(CASE WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden) >0 
		THEN (
		CONCAT((CASE WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.chko=true)>0 THEN 'O' 
		     WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.chka=true)>0 THEN 'A' 
		     WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.chkb=true)>0 THEN 'B' 
		     WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.chkab=true)>0 THEN 'AB' END),
			(CASE WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.rbrhpositivo=true)>0 THEN '+' 
			WHEN (select count(lb.n_orden) from lab_clinico as lb where lb.n_orden=noo.n_orden AND lb.rbrhnegativo=true)>0 THEN '-' END )
			)
		) 

		ELSE ( CASE WHEN (select nnn.gruposan from n_orden_ocupacional as nnn where nnn.n_orden=noo.n_orden) is null or 
		(select nnn.gruposan from n_orden_ocupacional as nnn where nnn.n_orden=noo.n_orden)='null' THEN 'N/A' 
		ELSE (select nnn.gruposan from n_orden_ocupacional as nnn where nnn.n_orden=noo.n_orden)  END) 
		END) AS grupo_sanguineo
			from n_orden_ocupacional as noo inner join datos_paciente as dp on dp.cod_pa= noo.cod_pa inner join sede_multisucursal as sm on
			noo.cod_sede=sm.id inner join contratas as cont on cont.razon_contrata=noo.razon_contrata
			where dp.cod_pa= CAST(dni_user as integer) and sm.id=clave
			and cont.ruc_contrata=ruc_contrata_user
			order by noo.fecha_apertura_po desc;
	

		end if;



	end if;

END; $function$
;

-- Permissions

ALTER FUNCTION public.listadodetallehistorialusuarios(varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar) OWNER TO pierola;
GRANT ALL ON FUNCTION public.listadodetallehistorialusuarios(varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar) TO pierola;

-- DROP FUNCTION public.listadohistorialusuarios(varchar, varchar, varchar, varchar, varchar, varchar, varchar);

CREATE OR REPLACE FUNCTION public.listadohistorialusuarios(user_name character varying, fecha_inicio character varying, fecha_fin character varying, tipo character varying, ruc_contrata_user character varying, ruc_empresa_user character varying, sede_user character varying)
 RETURNS TABLE(id bigint, codigo character varying, dni integer, apellido text, nombre text, fecha date)
 LANGUAGE plpgsql
AS $function$
	DECLARE clave INTEGER;

BEGIN
		select sm.id into  clave from sede_multisucursal AS sm where sm.codigo_sucursal=sede_user;

	if(tipo='EMPRESA' AND ruc_empresa_user='20477167561') then
		RETURN QUERY 
		
		select  DISTINCT ON (dni) row_number() over() as id,sm.codigo_sucursal as codigo,dp.cod_pa as dni, dp.apellidos_pa as apellido, dp.nombres_pa as nombre, noo.fecha_apertura_po as fecha 
		from datos_paciente as dp inner join n_orden_ocupacional as noo on dp.cod_pa=noo.cod_pa inner join sede_multisucursal as sm on
		noo.cod_sede=sm.id 
		where sm.id=clave and noo.fecha_apertura_pO BETWEEN to_date(fecha_inicio, 'YYYY-MM-DD') AND to_date(fecha_fin, 'YYYY-MM-DD')
		order by dni,noo.fecha_apertura_po desc;
	end if;

	if(tipo='EMPRESA' AND ruc_empresa_user<>'20477167561') then
		RETURN QUERY 

		select  DISTINCT ON (dni) row_number() over() as id,sm.codigo_sucursal as codigo,dp.cod_pa as dni, dp.apellidos_pa as apellido, dp.nombres_pa as nombre, noo.fecha_apertura_po as fecha 
		from datos_paciente as dp inner join n_orden_ocupacional as noo on dp.cod_pa=noo.cod_pa inner join sede_multisucursal as sm on
		noo.cod_sede=sm.id  inner join empresas as emp on emp.razon_empresa=noo.razon_empresa
		where sm.id=clave and noo.fecha_apertura_pO BETWEEN to_date(fecha_inicio, 'YYYY-MM-DD') AND to_date(fecha_fin, 'YYYY-MM-DD') and emp.ruc_empresa=ruc_empresa_user
		order by dni,noo.fecha_apertura_po desc;
		
	end if;


	if(tipo='CONTRATA') then
		RETURN QUERY 

		select  DISTINCT ON (dni) row_number() over() as id,sm.codigo_sucursal as codigo,dp.cod_pa as dni, dp.apellidos_pa as apellido, dp.nombres_pa as nombre, noo.fecha_apertura_po as fecha 
		from datos_paciente as dp inner join n_orden_ocupacional as noo on dp.cod_pa=noo.cod_pa inner join sede_multisucursal as sm on
		noo.cod_sede=sm.id  inner join contratas as cont on cont.razon_contrata=noo.razon_contrata
		where sm.id=clave and noo.fecha_apertura_pO BETWEEN to_date(fecha_inicio, 'YYYY-MM-DD') AND to_date(fecha_fin, 'YYYY-MM-DD') and cont.ruc_contrata=ruc_contrata_user
		order by dni,noo.fecha_apertura_po desc;
		
	end if;


END; $function$
;

-- Permissions

ALTER FUNCTION public.listadohistorialusuarios(varchar, varchar, varchar, varchar, varchar, varchar, varchar) OWNER TO pierola;
GRANT ALL ON FUNCTION public.listadohistorialusuarios(varchar, varchar, varchar, varchar, varchar, varchar, varchar) TO pierola;

-- DROP FUNCTION public.nombreprotocolo(int4);

CREATE OR REPLACE FUNCTION public.nombreprotocolo(codigo integer)
 RETURNS text
 LANGUAGE plpgsql
AS $function$
declare nambre text;

begin

nambre=(select pt.nombreprotocolo from historialClienteProtocolo as hcp left join protocoloServicio as ps on hcp.idprotocoloservicio
=ps.idprotocoloservicio left join protocolo as pt on ps.idprotocolo=pt.idprotocolo 
where hcp.n_orden=codigo limit 1);
RETURN nambre;

end
$function$
;

-- Permissions

ALTER FUNCTION public.nombreprotocolo(int4) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.nombreprotocolo(int4) TO huanchaco;

-- DROP FUNCTION public.obtener_edad(in date, in date, out int4);

CREATE OR REPLACE FUNCTION public.obtener_edad(pd_fecha_ini date, pd_fecha_fin date, OUT pn_edad integer)
 RETURNS integer
 LANGUAGE plpgsql
AS $function$
BEGIN
pn_edad := FLOOR(((DATE_PART('YEAR',pd_fecha_fin)-DATE_PART('YEAR',pd_fecha_ini))* 372 + (DATE_PART('MONTH',pd_fecha_fin) - DATE_PART('MONTH',pd_fecha_ini))*31 + (DATE_PART('DAY',pd_fecha_fin)-DATE_PART('DAY',pd_fecha_ini)))/372);
END;
$function$
;

-- Permissions

ALTER FUNCTION public.obtener_edad(in date, in date, out int4) OWNER TO postgres;
GRANT ALL ON FUNCTION public.obtener_edad(in date, in date, out int4) TO postgres;

-- DROP FUNCTION public.reactivoono(int4);

CREATE OR REPLACE FUNCTION public.reactivoono(codigo integer)
 RETURNS text
 LANGUAGE plpgsql
AS $function$
declare mensaje text;
declare valor1 numeric(10,2);
declare valor2 numeric(10,2);
declare valor3 numeric(10,2);
declare a1 boolean;
declare a2 boolean;
declare a3 boolean;
declare a4 boolean;

begin
valor1=(select n_orden from n_orden_ocupacional where UPPER(nom_examen) LIKE '%CUANTITATIVA ANTIGENOS%' and n_orden=codigo);
valor2=(select n_orden from n_orden_ocupacional where UPPER(nom_examen) LIKE '%ANTIGENOS%' and n_orden=codigo);
valor3=(select n_orden from n_orden_ocupacional where UPPER(nom_examen) LIKE '%COVID-19%' and n_orden=codigo);

mensaje=' ';
if(valor1 IS NULL and valor3 is null  )
then
mensaje=(select (case when chkigm_reactivo='true' then 'REACTIVO'else 'NO REACTIVO' end ) FROM 
		n_orden_ocupacional LEFT JOIN examen_inmunologico ON n_orden_ocupacional.n_orden = examen_inmunologico.n_orden
		 where n_orden_ocupacional.n_orden=codigo);
else
mensaje=(select (case when valorigm>=0.04 then 'REACTIVO'else 'NO REACTIVO' end ) FROM 
		n_orden_ocupacional LEFT JOIN examen_inmunologico ON n_orden_ocupacional.n_orden = examen_inmunologico.n_orden
		 where n_orden_ocupacional.n_orden=codigo);
end if;
if(valor2 IS NULL)
then
a1=(select chkigm_reactivo from examen_inmunologico where n_orden=codigo );
a2=(select chkigm_noreactivo from examen_inmunologico where n_orden=codigo );
a3=(select chkigg_reactivo from examen_inmunologico where n_orden=codigo );
a4=(select chkigg_noreactivo from examen_inmunologico where n_orden=codigo );
if(a1='true' AND a3='true')
then
mensaje='IGM/IGG REACTIVO';
end if;
if(a2='true' and a3='true' )
then
mensaje='IGG REACTIVO';
end if;
if(a1='true' AND a4='true')
then
mensaje='IGM REACTIVO';
end if;

if(a2='true' and a4='true')
then
mensaje='NO REACTIVO';
end if;
else 
mensaje=mensaje;
end if;


RETURN mensaje;

end
$function$
;

-- Permissions

ALTER FUNCTION public.reactivoono(int4) OWNER TO huanchaco;
GRANT ALL ON FUNCTION public.reactivoono(int4) TO huanchaco;

-- DROP FUNCTION public.registro_automatico_historiaclinica(int8, int8, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, bool, bool, bool, int8, varchar, varchar, varchar, bool, bool, bool, bool, bool, varchar, varchar, int8, varchar, varchar, varchar, varchar, varchar, varchar, varchar, bool, bool, bool, bool, bool, bool, varchar);

CREATE OR REPLACE FUNCTION public.registro_automatico_historiaclinica(n_orden_exp bigint, cod_pa_exp bigint, razon_empresa_exp character varying, razon_contrata_exp character varying, nom_ex_exp character varying, altura_po_exp character varying, mineral_po_exp character varying, fecha_apertura_po_exp character varying, precio_po_exp character varying, estado_ex_exp character varying, nom_examen_exp character varying, cargo_de_exp character varying, area_o_exp character varying, n_medico_exp character varying, n_hora_exp character varying, tipo_pago_exp character varying, n_fisttest_exp boolean, n_psicosen_exp boolean, n_testaltura_exp boolean, color_exp bigint, grupo_san_exp character varying, grupo_factor_san_exp character varying, cod_clinica_exp character varying, visual_compl_exp boolean, trab_calientes_exp boolean, chk_covid1_exp boolean, chk_covid2_exp boolean, manip_alimentos_exp boolean, txtobserv1_exp character varying, txtobserv2_exp character varying, cod_sede_exp bigint, tipo_prueba_covid_exp character varying, tipo_prueba_exp character varying, nombre_hotel_exp character varying, protocolo_exp character varying, precio_adic_exp character varying, autoriza_exp character varying, n_operacion_exp character varying, herra_manuales_exp boolean, rxc_dorso_lumbar_exp boolean, rxc_lumbar_exp boolean, rxc_lumbosacra_exp boolean, rxc_plomos_exp boolean, mercurioo_exp boolean, referencia_exp character varying)
 RETURNS TABLE(id_resp bigint, mensaje text)
 LANGUAGE plpgsql
AS $function$
	DECLARE validar_hc_hm BIGINT;
	DECLARE n_orden_hm BIGINT;


BEGIN
	select count(no.n_orden) into validar_hc_hm from n_orden_ocupacional as no where no.referencia=CAST(n_orden_exp AS TEXT) and no.cod_sede=cod_sede_exp;
		
	if(validar_hc_hm>0) then
		update n_orden_ocupacional set cod_pa=cod_pa_exp,razon_empresa=razon_empresa_exp,razon_contrata=razon_contrata_exp,nom_ex=nom_ex_exp,
		altura_po=altura_po_exp,mineral_po=mineral_po_exp,fecha_apertura_po=TO_DATE(fecha_apertura_po_exp,'YYYY-MM-DD'),precio_po=CAST(precio_po_exp as money),
		estado_ex=estado_ex_exp,nom_examen=nom_examen_exp,cargo_de=cargo_de_exp,area_o=area_o_exp,n_medico=n_medico_exp,n_hora=CAST (n_hora_exp as time without time zone),
		tipo_pago=tipo_pago_exp,n_fisttest=n_fisttest_exp,n_psicosen=n_psicosen_exp,n_testaltura=n_testaltura_exp,color=color_exp,gruposan=grupo_san_exp,grupofactorsan=grupo_factor_san_exp,
		cod_clinica=cod_clinica_exp,visual_compl=visual_compl_exp,trab_calientes=trab_calientes_exp,chkcovid1=chk_covid1_exp,chkcovid2=chk_covid2_exp,manip_alimentos=manip_alimentos_exp,
		txtobserv1=txtobserv1_exp,txtobserv2=txtobserv2_exp,cod_sede=cod_sede_exp,tipo_prueba_covid=tipo_prueba_covid_exp,tipoPrueba=tipo_prueba_exp,nombrehotel=nombre_hotel_exp,
		protocolo=protocolo_exp,precio_adic=CAST(precio_adic_exp AS money),autoriza=autoriza_exp,n_operacion=n_operacion_exp, herra_manuales=herra_manuales_exp, rxc_dorso_lumbar=rxc_dorso_lumbar_exp,
		 rxc_lumbar=rxc_lumbar_exp, rxc_lumbosacra=rxc_lumbosacra_exp, rxc_plomos=rxc_plomos_exp, mercurioo=mercurioo_exp,referencia=referencia_exp where referencia=CAST(n_orden_exp AS TEXT) and cod_sede=cod_sede_exp;
		RETURN QUERY 
		select CAST(1 AS bigint) as id_resp, CAST('El usuario se actualizo con exito!' AS TEXT) as mensaje;
		
 	else
	
		INSERT INTO n_orden_ocupacional(cod_pa, razon_empresa, razon_contrata,
                        nom_ex, altura_po,mineral_po, fecha_apertura_po,precio_po,estado_ex,nom_examen,cargo_de,area_o,n_medico,n_hora,
                        tipo_pago,n_fisttest,n_psicosen,n_testaltura,color,gruposan,grupofactorsan,cod_clinica,visual_compl,
                        trab_calientes,chkcovid1,chkcovid2,manip_alimentos, txtobserv1,txtobserv2,cod_sede,tipo_prueba_covid,tipoPrueba,nombrehotel,protocolo,
                        precio_adic,autoriza,n_operacion, herra_manuales, rxc_dorso_lumbar, rxc_lumbar, rxc_lumbosacra, rxc_plomos,mercurioo,referencia)
		values (cod_pa_exp, razon_empresa_exp, razon_contrata_exp,nom_ex_exp, altura_po_exp, mineral_po_exp, TO_DATE(fecha_apertura_po_exp,'YYYY-MM-DD'), CAST(precio_po_exp as money), estado_ex_exp,
	nom_examen_exp, cargo_de_exp, area_o_exp,n_medico_exp, CAST (n_hora_exp as time without time zone), tipo_pago_exp,n_fisttest_exp, n_psicosen_exp,n_testaltura_exp, color_exp, grupo_san_exp,grupo_factor_san_exp, 
	cod_clinica_exp,visual_compl_exp, trab_calientes_exp,chk_covid1_exp,chk_covid2_exp,manip_alimentos_exp,txtobserv1_exp,txtobserv2_exp,cod_sede_exp,tipo_prueba_covid_exp ,tipo_prueba_exp,nombre_hotel_exp,
	protocolo_exp,CAST(precio_adic_exp AS money),autoriza_exp,n_operacion_exp,herra_manuales_exp, rxc_dorso_lumbar_exp, rxc_lumbar_exp,rxc_lumbosacra_exp ,rxc_plomos_exp, mercurioo_exp , referencia_exp ) RETURNING n_orden into n_orden_hm;
                        
		RETURN QUERY 
		select CAST(n_orden_hm as bigint) as id_resp, CAST('El usuario se registro con exito!' AS TEXT) as mensaje;
	end if;

END; $function$
;

-- Permissions

ALTER FUNCTION public.registro_automatico_historiaclinica(int8, int8, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, bool, bool, bool, int8, varchar, varchar, varchar, bool, bool, bool, bool, bool, varchar, varchar, int8, varchar, varchar, varchar, varchar, varchar, varchar, varchar, bool, bool, bool, bool, bool, bool, varchar) OWNER TO pierola;
GRANT ALL ON FUNCTION public.registro_automatico_historiaclinica(int8, int8, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, bool, bool, bool, int8, varchar, varchar, varchar, bool, bool, bool, bool, bool, varchar, varchar, int8, varchar, varchar, varchar, varchar, varchar, varchar, varchar, bool, bool, bool, bool, bool, bool, varchar) TO pierola;

-- DROP FUNCTION public.registro_automatico_huamachuco(int8, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, int8, varchar, varchar, varchar, varchar, varchar, text, int8);

CREATE OR REPLACE FUNCTION public.registro_automatico_huamachuco(cod_pa_exp bigint, nombres_pa_exp character varying, fecha_nacimiento_pa_exp character varying, sexo_pa_exp character varying, email_pa_exp character varying, lugar_nac_pa_exp character varying, nivel_est_pa_exp character varying, ocupacion_pa_exp character varying, estado_civil_pa_exp character varying, direccion_pa_exp character varying, departamento_pa_exp character varying, provincia_pa_exp character varying, distrito_pa_exp character varying, caserio_pa_exp character varying, foto_pa_exp character varying, cod_aleatorio_pa_exp bigint, tel_casa_pa_exp character varying, tel_trabajo_pa_exp character varying, cel_pa_exp character varying, fecha_registro_pa_exp character varying, apellidos_pa_exp character varying, hora_registro_pa_exp text, tipo_doc_exp bigint)
 RETURNS TABLE(id_resp integer, mensaje text)
 LANGUAGE plpgsql
AS $function$
	DECLARE validar_pasiente_hm BIGINT;

BEGIN
	select count(dp.cod_pa) into validar_pasiente_hm from datos_paciente as dp where dp.cod_pa= cod_pa_exp;
		
	if(validar_pasiente_hm>0) then
		update datos_paciente set nombres_pa=nombres_pa_exp,fecha_nacimiento_pa=TO_DATE(fecha_nacimiento_pa_exp,'YYYY-MM-DD'),sexo_pa=Cast(sexo_pa_exp as char),email_pa=
		email_pa_exp,lugar_nac_pa=lugar_nac_pa_exp,nivel_est_pa=nivel_est_pa_exp,ocupacion_pa=ocupacion_pa_exp,estado_civil_pa=estado_civil_pa_exp,
		direccion_pa=direccion_pa_exp,departamento_pa=departamento_pa_exp,provincia_pa=provincia_pa_exp,distrito_pa=distrito_pa_exp,
		caserio_pa=caserio_pa_exp,foto_pa=CAST(foto_pa_exp AS bytea),cod_aleatorio_pa=cod_aleatorio_pa_exp,tel_casa_pa=tel_casa_pa_exp,
		tel_trabajo_pa=tel_trabajo_pa_exp,cel_pa=cel_pa_exp,fecha_registro_pa=TO_DATE(fecha_registro_pa_exp,'YYYY-MM-DD'),apellidos_pa=apellidos_pa_exp,
		hora_registro_pa=CAST(hora_registro_pa_exp AS time without time zone),tipo_doc=tipo_doc_exp WHERE cod_pa=cod_pa_exp;
		RETURN QUERY 
		select 1 as id_resp, CAST('El usuario se actualizo con exito!' AS TEXT) as mensaje;
		
 	else
	
		Insert INTO datos_paciente (cod_pa,nombres_pa,fecha_nacimiento_pa,sexo_pa,email_pa,lugar_nac_pa,nivel_est_pa,ocupacion_pa,
		estado_civil_pa,direccion_pa,departamento_pa,provincia_pa,distrito_pa,caserio_pa,foto_pa,cod_aleatorio_pa,
		tel_casa_pa,
		tel_trabajo_pa,cel_pa,fecha_registro_pa, apellidos_pa, hora_registro_pa, tipo_doc) values (cod_pa_exp,nombres_pa_exp,
		TO_DATE(fecha_nacimiento_pa_exp,'YYYY-MM-DD'),Cast(sexo_pa_exp as char),email_pa_exp,lugar_nac_pa_exp,nivel_est_pa_exp,ocupacion_pa_exp,estado_civil_pa_exp,
		direccion_pa_exp,departamento_pa_exp,provincia_pa_exp,distrito_pa_exp,caserio_pa_exp,CAST(foto_pa_exp AS bytea),cod_aleatorio_pa_exp,tel_casa_pa_exp,tel_trabajo_pa_exp,cel_pa_exp,TO_DATE(fecha_registro_pa_exp,'YYYY-MM-DD'),apellidos_pa_exp,
		CAST(hora_registro_pa_exp AS time without time zone) ,tipo_doc_exp);
		RETURN QUERY 
		select 2 as id_resp, CAST('El usuario se registro con exito!' AS TEXT) as mensaje;
	end if;

END; $function$
;

-- Permissions

ALTER FUNCTION public.registro_automatico_huamachuco(int8, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, int8, varchar, varchar, varchar, varchar, varchar, text, int8) OWNER TO pierola;
GRANT ALL ON FUNCTION public.registro_automatico_huamachuco(int8, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, varchar, int8, varchar, varchar, varchar, varchar, varchar, text, int8) TO pierola;

-- DROP FUNCTION public.sp_ejemplo(varchar);

CREATE OR REPLACE FUNCTION public.sp_ejemplo(filtro character varying)
 RETURNS TABLE(n_orden text, razon_contrata text)
 LANGUAGE plpgsql
AS $function$
BEGIN
 RETURN QUERY 
	 SELECT
	 	 a.n_orden,
		 a.razon_contrata
	 FROM
		 n_orden_ocupacional a
	 WHERE
		 a.razon_empresa ILIKE filtro;
END; $function$
;

-- Permissions

ALTER FUNCTION public.sp_ejemplo(varchar) OWNER TO postgres;
GRANT ALL ON FUNCTION public.sp_ejemplo(varchar) TO postgres;

-- DROP FUNCTION public.sp_matriz_general(text, text, text, text);

CREATE OR REPLACE FUNCTION public.sp_matriz_general(ruc_empresa_param text, ruc_contrata_param text, f_inicio text, f_fin text)
 RETURNS TABLE(n integer, condicion text, fechadigitacion text, razon_empresa text, razon_contrata text, apellidos_nombres text, fechanacimiento text, edad integer, sexo text, dni_carnet integer, puesto text, celular text, establecimiento text, tipodeemo text, fecha_de_evaluacion text, vigenciahasta text, o_d_500 text, o_d_1000 text, o_d_2000 text, o_d_3000 text, o_d_4000 text, o_d_6000 text, o_d_8000 text, promedio_od text, o_i_500 text, o_i_1000 text, o_i_2000 text, o_i_3000 text, o_i_4000 text, o_i_6000 text, o_i_8000 text, promedio_oi text, o_d1_500 text, o_d1_1000 text, o_d1_2000 text, o_d1_3000 text, o_d1_4000 text, o_d1_6000 text, o_d1_8000 text, o_i1_500 text, o_i1_1000 text, o_i1_2000 text, o_i1_3000 text, o_i1_4000 text, o_i1_6000 text, o_i1_8000 text, conclusion text, txtconclusionesradiograficas text, conclusionesradiograficasoit text, fvc text, fev1 text, fev1fvc text, txtconclusion text, peso text, talla text, imc text, r_imc text, cerca_od text, cerca_oi text, cerca_corregida_od text, cerca_corregida_oi text, lejos_od text, lejos_oi text, lejos_corregida_od text, lejos_corregida_oi text, estereopsis text, e_oculares text, e_oculares1 text, v_colores text, hemoglobina text, conclucion_hemoglobina text, glucosa text, conclucion_glucosa text, colesterol text, conclucion_colesterol text, ldl_colesterol text, conclucion_ldl_colesterol text, hdl_colesterol text, conclucion_hdl_colesterol text, vldl_colesterol text, conclucion_vldl_colesterol text, trigliceridos text, conclucion_trigliseridos text, creatinina text, conclucion_creatinina text, cintura text, cadera text, icc text, sistolica text, r_sist text, diastolica text, r_diast text, fc text, fr text, plomo_sangre text, cadmio_orina text, tiocinatos_orina text, ekg_resultado text, resultadodeevaluaciones text, grupoyfactor text, alergias text, aptitud_trab_altura_basico text, aptitud_trab_altura_mineria text, aptitud_manejo text, aptitud_ocup text, n_medico text, a_intelectual text, a_personalidad text, recomendaciones text, aptitudpsicologica text, diagnostico_musculo_esqueletico text, recomendacion_musculo_esqueletico text, conclusionesaptitud text, recomendacionesaptitud text, restriccionesaptitud text)
 LANGUAGE plpgsql
AS $function$
BEGIN
	RETURN QUERY
SELECT n.n_orden AS N, 
	CASE WHEN ob.n_orden is not null THEN 'Observado'
             WHEN ca.chkapto = 'TRUE' THEN 'Apto'
             WHEN ca1.chkapto = 'TRUE' THEN 'Apto'
             WHEN ca.chkapto_restriccion = 'TRUE' THEN 'Apto con Restriccion'
             WHEN ca1.chkapto_restriccion = 'TRUE' THEN 'Apto con Restriccion'
	     WHEN ama.chkapto = 'TRUE' THEN 'Apto'
             WHEN ama.chkapto_restriccion = 'TRUE' THEN 'Apto con Restriccion'
             WHEN ca1.chkevaluado = 'TRUE' THEN 'Evaluado'
             WHEN ca1.chkconobservacion = 'TRUE' THEN 'Con observacion'
             WHEN ca.chkno_apto = 'TRUE' THEN 'No Apto'
             WHEN ca1.chkno_apto = 'TRUE' THEN 'No Apto'
             WHEN ama.chkno_apto = 'TRUE' THEN 'No Apto'
             WHEN ama.n_orden IS NULL AND ca.n_orden IS NULL THEN 'NO REGISTRO APTITUD'
              END as CONDICION,
        CAST(fecha_apertura_po AS TEXT) AS FECHADIGITACION,  n.razon_empresa AS razon_empresa,n.razon_contrata,     
        trim(d.apellidos_pa)|| ' '|| trim(nombres_pa) AS apellidos_nombres,CAST(d.fecha_nacimiento_pa AS TEXT) AS FECHANACIMIENTO,
        obtener_edad(d.fecha_nacimiento_pa,n.fecha_apertura_po) AS EDAD,
        CASE WHEN d.sexo_pa='F' THEN 'FEMENINO' ELSE 'MASCULINO' END AS SEXO,
	d.cod_pa AS dni_carnet,n.cargo_de AS PUESTO,d.cel_pa as celular,
	CASE WHEN n.razon_empresa !='' THEN 'HORIZONTE MEDIC' END  AS ESTABLECIMIENTO,
	n.nom_examen AS TIPODEEMO, CAST(fecha_apertura_po AS TEXT) AS fecha_de_evaluacion,
	CAST((CASE WHEN ca.fecha_hasta IS NOT NULL THEN ca.fecha_hasta 
	     WHEN ama.fecha_hasta IS NOT NULL THEN ama.fecha_hasta
	      END) AS TEXT) AS VIGENCIAHASTA,
	CASE WHEN au.n_orden is not null THEN au.o_d_500
             WHEN au1.n_orden is not null THEN au1.o_d_500
             WHEN au3.n_orden is not null  THEN au3.o_d_500
             END AS o_d_500,	
	CASE WHEN au.n_orden is not null THEN au.o_d_1000
             WHEN au1.n_orden is not null THEN au1.o_d_1000
             WHEN au3.n_orden is not null  THEN au3.o_d_1000
             END AS o_d_1000,	
	CASE WHEN au.n_orden is not null THEN au.o_d_2000
             WHEN au1.n_orden is not null THEN au1.o_d_2000
             WHEN au3.n_orden is not null  THEN au3.o_d_2000
             END AS o_d_2000,
        CASE WHEN au.n_orden is not null THEN au.o_d_3000
             WHEN au1.n_orden is not null THEN au1.o_d_3000
             WHEN au3.n_orden is not null  THEN au3.o_d_3000
             END AS o_d_3000,
        CASE WHEN au.n_orden is not null THEN au.o_d_4000
             WHEN au1.n_orden is not null THEN au1.o_d_4000
             WHEN au3.n_orden is not null  THEN au3.o_d_4000
             END AS o_d_4000,
        CASE WHEN au.n_orden is not null THEN au.o_d_6000
             WHEN au1.n_orden is not null THEN au1.o_d_6000
             WHEN au3.n_orden is not null  THEN au3.o_d_6000
             END AS o_d_6000,  
        CASE WHEN au.n_orden is not null THEN au.o_d_8000
             WHEN au1.n_orden is not null THEN au1.o_d_8000
             WHEN au3.n_orden is not null  THEN au3.o_d_8000
             END AS o_d_8000,  
	CASE WHEN n.n_orden is not null THEN '...' END PROMEDIO_OD,
	CASE WHEN au.n_orden is not null THEN au.o_i_500
             WHEN au1.n_orden is not null THEN au1.o_i_500
             WHEN au3.n_orden is not null  THEN au3.o_i_500
             END AS o_i_500, 
	CASE WHEN au.n_orden is not null THEN au.o_i_1000
             WHEN au1.n_orden is not null THEN au1.o_i_1000
             WHEN au3.n_orden is not null  THEN au3.o_i_1000
             END AS o_i_1000, 
        CASE WHEN au.n_orden is not null THEN au.o_i_2000
             WHEN au1.n_orden is not null THEN au1.o_i_2000
             WHEN au3.n_orden is not null  THEN au3.o_i_2000
             END AS o_i_2000,   
        CASE WHEN au.n_orden is not null THEN au.o_i_3000
             WHEN au1.n_orden is not null THEN au1.o_i_3000
             WHEN au3.n_orden is not null  THEN au3.o_i_3000
             END AS o_i_3000,     
	CASE WHEN au.n_orden is not null THEN au.o_i_4000
             WHEN au1.n_orden is not null THEN au1.o_i_4000
             WHEN au3.n_orden is not null  THEN au3.o_i_4000
             END AS o_i_4000,     
        CASE WHEN au.n_orden is not null THEN au.o_i_6000
             WHEN au1.n_orden is not null THEN au1.o_i_6000
             WHEN au3.n_orden is not null  THEN au3.o_i_6000
             END AS o_i_6000, 
        CASE WHEN au.n_orden is not null THEN au.o_i_8000
             WHEN au1.n_orden is not null THEN au1.o_i_8000
             WHEN au3.n_orden is not null  THEN au3.o_i_8000
             END AS o_i_8000,  
	CASE WHEN n.n_orden is not null THEN '...' END PROMEDIO_OI,
	 CASE WHEN au.n_orden is not null THEN au.o_d1_500
             WHEN au1.n_orden is not null THEN au1.o_d1_500
             WHEN au3.n_orden is not null  THEN au3.o_d1_500
             END AS o_d1_500,
         CASE WHEN au.n_orden is not null THEN au.o_d1_1000
             WHEN au1.n_orden is not null THEN au1.o_d1_1000
             WHEN au3.n_orden is not null  THEN au3.o_d1_1000
             END AS o_d1_1000,    
	CASE WHEN au.n_orden is not null THEN au.o_d1_2000
             WHEN au1.n_orden is not null THEN au1.o_d1_2000
             WHEN au3.n_orden is not null  THEN au3.o_d1_2000
             END AS o_d1_2000,
        CASE WHEN au.n_orden is not null THEN au.o_d1_3000
             WHEN au1.n_orden is not null THEN au1.o_d1_3000
             WHEN au3.n_orden is not null  THEN au3.o_d1_3000
             END AS o_d1_3000,     
	CASE WHEN au.n_orden is not null THEN au.o_d1_4000
             WHEN au1.n_orden is not null THEN au1.o_d1_4000
             WHEN au3.n_orden is null  THEN au3.o_d1_4000
             END AS o_d1_4000,   
        CASE WHEN au.n_orden is not null THEN au.o_d1_6000
             WHEN au1.n_orden is not null THEN au1.o_d1_6000
             WHEN au3.n_orden is not null  THEN au3.o_d1_6000
             END AS o_d1_6000,      
        CASE WHEN au.n_orden is not null THEN au.o_d1_8000
             WHEN au1.n_orden is not null THEN au1.o_d1_8000
             WHEN au3.n_orden is not null  THEN au3.o_d1_8000
             END AS o_d1_8000,  
        CASE WHEN au.n_orden is not null THEN au.o_i1_500
             WHEN au1.n_orden is not null THEN au1.o_i1_500
             WHEN au3.n_orden is not null  THEN au3.o_i1_500
             END AS o_i1_500, 
        CASE WHEN au.n_orden is not null THEN au.o_i1_1000
             WHEN au1.n_orden is not null THEN au1.o_i1_1000
             WHEN au3.n_orden is not null  THEN au3.o_i1_1000
             END AS o_i1_1000, 
        CASE WHEN au.n_orden is not null THEN au.o_i1_2000
             WHEN au1.n_orden is not null THEN au1.o_i1_2000
             WHEN au3.n_orden is not null  THEN au3.o_i1_2000
             END AS o_i1_2000,     
        CASE WHEN au.n_orden is not null THEN au.o_i1_3000
             WHEN au1.n_orden is not null THEN au1.o_i1_3000
             WHEN au3.n_orden is not null  THEN au3.o_i1_3000
             END AS o_i1_3000,  
        CASE WHEN au.n_orden is not null THEN au.o_i1_4000
             WHEN au1.n_orden is not null THEN au1.o_i1_4000
             WHEN au3.n_orden is not null  THEN au3.o_i1_4000
             END AS o_i1_4000,  
        CASE WHEN au.n_orden is not null THEN au.o_i1_6000
             WHEN au1.n_orden is not null THEN au1.o_i1_6000
             WHEN au3.n_orden is not null  THEN au3.o_i1_6000
             END AS o_i1_6000,
        CASE WHEN au.n_orden is not null THEN au.o_i1_8000
             WHEN au1.n_orden is not null THEN au1.o_i1_8000
             WHEN au3.n_orden is not null  THEN au3.o_i1_8000
             END AS o_i1_8000,  
        CASE WHEN au.n_orden is not null THEN au.diagnostico 
             WHEN au3.n_orden is not null  THEN au3.txtdiag_od ||' '|| au3.txtdiag_od 
             WHEN au1.chkdnormal='TRUE' THEN 'NORMAL '
             WHEN au1.chkdtaleveod='TRUE' THEN 'Trauma Acstico Leve OD '
             WHEN au1.chkdtaavanzadood='TRUE' THEN 'Trauma Acstico Avanzado OD '
             WHEN au1.chkdhrleveod='TRUE' THEN 'Hipoacusia Inducida por Ruido, Leve OD '
             WHEN au1.chkdhrmoderadood='TRUE' THEN 'Hipoacusia Inducida por Ruido, Moderada OD '
             WHEN au1.chkdhravanzadaod='TRUE' THEN 'Hipoacusia Inducida por Ruido, Avanzada  OD '
            -- ELSE '.' END ||''|| 
	--CASE 
	     WHEN au1.chkdtaleveoi='TRUE' THEN 'Trauma Acstico Leve OI'
             WHEN au1.chkdtaavanzadooi='TRUE' THEN 'Trauma Acstico Avanzado OI'
             WHEN au1.chkdhrleveoi='TRUE' THEN 'Hipoacusia Inducida por Ruido, Leve OI'
             WHEN au1.chkdhrmoderadooi='TRUE' THEN 'Hipoacusia Inducida por Ruido, Moderada OI'
             WHEN au1.chkdhravanzadaoi='TRUE' THEN 'Hipoacusia Inducida por Ruido, Avanzada  OI'
             WHEN (au1.n_orden is null and au.n_orden is null and au3.n_orden is null) THEN 'NO SE REALIZO AUDIOMETRIA'
             --else 'NO SE REALIZO AUDIOMETRIA' 

             END CONCLUSION,
        CASE WHEN rt.n_orden is null then 'NO SE REGISTRO INF. RADIGR.' else rt.txtconclusionesradiograficas end,
        CASE WHEN oi.n_orden is null then 'NO SE REGISTRO FICHA OIT' else oi.txt_s_comentarios end AS CONCLUSIONESRADIOGRAFICASOIT,
        CASE WHEN f.fvc is null THEN 'INF. NO TOMADA' ELSE f.fvc END,
        CASE WHEN f.fev1 is null THEN 'INF. NO TOMADA' ELSE f.fev1 END,
        CASE WHEN f.fev1fvc is null THEN 'INF. NO TOMADA' ELSE f.fev1fvc END,
        CASE WHEN f.fvc='N/A' or f.n_orden is null THEN 'NO SE REALIZO ESPIROMETRIA' ELSE a.txtconclusion END,	
	CASE WHEN t.peso is null THEN 'INF. NO TOMADA' ELSE t.peso END,
	CASE WHEN t.talla is null THEN 'INF. NO TOMADA' ELSE t.talla END,
	CASE WHEN t.imc is null THEN 'INF. NO TOMADA' ELSE t.imc END,
	CASE WHEN t.imc < '18.5' THEN 'PESO BAJO'
	     WHEN t.imc > '18.4' AND t.imc < '25'  THEN 'NORMAL'
	     WHEN t.imc > '24.9' AND t.imc < '30'  THEN 'SOBREPESO'   
	     WHEN t.imc > '29.9' AND t.imc < '35' THEN 'OBESIDAD I'
	     WHEN t.imc > '34.9' AND t.imc < '40' THEN 'OBESIDAD II'
	     WHEN t.imc > '39.9' THEN 'OBESIDAD'
	     WHEN t.imc is null THEN 'SIN FICHA 7C'
	     END  AS R_IMC,
	CASE WHEN o.n_orden is not null THEN o.v_cerca_s_od
             WHEN ol1.n_orden is not null THEN ol1.txtCercaSinCorregirOD
             END AS Cerca_OD,  
	CASE WHEN o.n_orden is not null THEN o.v_cerca_s_oi
             WHEN ol1.n_orden is not null THEN ol1.txtcercasincorregiroi
             END AS Cerca_OI,  
        CASE WHEN o.n_orden is not null and o.v_cerca_c_od ='00' THEN ol.v_cerca_c_od
             WHEN ol1.n_orden is not null THEN ol1.txtcercacorregidaod
             else o.v_cerca_c_od
             END AS Cerca_Corregida_OD,  
        CASE WHEN o.n_orden is not null and o.v_cerca_c_oi ='00' THEN ol.v_cerca_c_oi
             WHEN ol1.n_orden is not null THEN ol1.txtcercacorregidaoi
             else o.v_cerca_c_oi
             END AS Cerca_Corregida_OI,  
	CASE WHEN o.n_orden is not null THEN o.v_lejos_s_od
             WHEN ol1.n_orden is not null THEN ol1.txtlejossincorregirod
             END AS Lejos_OD, 
        CASE WHEN o.n_orden is not null THEN o.v_lejos_s_oi
             WHEN ol1.n_orden is not null THEN ol1.txtlejossincorregiroi
             END AS Lejos_OI,     
	CASE WHEN o.n_orden is not null and o.v_lejos_c_od ='00' THEN ol.v_lejos_c_od
             WHEN ol1.n_orden is not null THEN ol1.txtlejoscorregidaod
             else o.v_lejos_c_od
             END AS Lejos_Corregida_OD,  
        CASE WHEN o.n_orden is not null and o.v_lejos_c_oi ='00' THEN ol.v_lejos_c_oi
             WHEN ol1.n_orden is not null THEN ol1.txtlejoscorregidaoi
             else o.v_lejos_c_oi
             END AS Lejos_Corregida_OI,    
	CASE WHEN n.n_orden IS NOT NULL THEN 'N/A'
	     ELSE 'NO REGISTRO EVAL.MUSC.ESQUELETICA' END AS ESTEREOPSIS,
	CASE WHEN ol1.n_orden IS NOT NULL THEN ol1.txtdiagnostico
	     WHEN ol.e_oculares is null THEN o.e_oculares 
	     ELSE ol.e_oculares END AS e_oculares,
	CASE WHEN ol.e_oculares1 IS NULL THEN o.e_oculares1
	     WHEN ol1.n_orden is NOT null THEN ol1.txtdiagnostico ELSE ol.e_oculares1  END,
	CASE WHEN o.n_orden IS not NULL THEN o.v_colores
	     WHEN ol1.n_orden is null THEN ol1.txtdiagnostico 
	     WHEN ol1.rbteccoleres_normal='TRUE' THEN 'NORMAL' 
	     WHEN ol1.rbteccoleres_anormal='TRUE' THEN 'ANORMAL' 
	     WHEN ol1.rbteccoleres_nc='TRUE' THEN 'N.C.' 
	     ELSE ol.e_oculares1  END AS v_colores,
	CASE WHEN lc.txthemoglobina IS NULL THEN 'INF. NO TOMADA' ELSE lc.txthemoglobina END AS Hemoglobina ,
	CASE WHEN (lc.txthemoglobina < '13' or lc.txthemoglobina > '20')AND d.sexo_pa='M' THEN 'ANORMAL' 
	     WHEN (lc.txthemoglobina < '12' or lc.txthemoglobina > '20')AND d.sexo_pa='F' THEN 'ANORMAL' 
	     WHEN (lc.txthemoglobina >= '13' AND lc.txthemoglobina <='20')AND d.sexo_pa='M'THEN 'NORMAL' 
	     WHEN (lc.txthemoglobina >= '12' AND lc.txthemoglobina <='20')AND d.sexo_pa='F' THEN 'NORMAL' 
	     WHEN lc.txthemoglobina IS NULL THEN 'INF. NO TOMADA'
	     END AS CONCLUCION_HEMOGLOBINA ,
	CASE WHEN lc.txtglucosabio IS NULL THEN 'INF. NO TOMADA' ELSE lc.txtglucosabio END AS Glucosa,
	CASE WHEN lc.txtglucosabio = 'N/A' OR lc.txtglucosabio = ''OR lc.txtglucosabio = '-'OR lc.txtglucosabio = '.' or lc.txtglucosabio is null THEN 'INF. NO TOMADA'
             WHEN lc.txtglucosabio::decimal <'70' or lc.txtglucosabio::decimal >'110' THEN 'ANORMAL' 
             WHEN lc.txtglucosabio::decimal >='70' AND lc.txtglucosabio::decimal<='110' THEN 'NORMAL' 
	     END AS CONCLUCION_GLUCOSA  ,
	CASE WHEN ab.txtcolesterol IS NOT NULL THEN ab.txtcolesterol ELSE 'INF. NO TOMADA'
	     END  AS Colesterol,   
	CASE WHEN ab.txtcolesterol= '' or ab.txtcolesterol= '-' or ab.txtldlcolesterol= '.' or ab.txtcolesterol= 'N/A' or ab.txtcolesterol IS NULL THEN 'INF. NO TOMADA' 
	     WHEN ab.txtcolesterol::decimal < '200' THEN 'NORMAL' 
	     WHEN ab.txtcolesterol::decimal >= '200'  THEN 'ANORMAL' 
	     END AS CONCLUCION_COLESTEROL,    
	CASE WHEN ab.txtldlcolesterol IS NOT NULL THEN ab.txtldlcolesterol ELSE 'INF. NO TOMADA'
	     END  AS LDL_COLESTEROL,  
	CASE WHEN ab.txtldlcolesterol= '' or ab.txtldlcolesterol= '-' or ab.txtldlcolesterol= '.'or ab.txtldlcolesterol= 'N/A' or ab.txtldlcolesterol IS NULL THEN 'INF. NO TOMADA' 
	     WHEN ab.txtldlcolesterol::decimal = '0' THEN 'INF.NO TOMADA'
	     WHEN ab.txtldlcolesterol::decimal < '129' THEN 'NORMAL' 
	     WHEN ab.txtldlcolesterol::decimal >= '129'  THEN 'ANORMAL' 
	     END AS CONCLUCION_LDL_COLESTEROL,     
	CASE WHEN ab.txthdlcolesterol IS NOT NULL THEN ab.txthdlcolesterol ELSE 'INF. NO TOMADA'
	     END  AS HDL_Colesterol,   
	CASE WHEN ab.txthdlcolesterol= '' or ab.txthdlcolesterol= '-' or ab.txthdlcolesterol= '.'or ab.txthdlcolesterol= 'N/A' or ab.txthdlcolesterol IS NULL THEN 'INF. NO TOMADA' 
             WHEN ab.txthdlcolesterol::decimal = '0' THEN 'INF.NO TOMADA'
	     WHEN ab.txthdlcolesterol::decimal < '40' or ab.txthdlcolesterol::decimal > '60'  THEN 'ANORMAL' 
	     WHEN ab.txthdlcolesterol::decimal >= '40' AND ab.txthdlcolesterol::decimal<='60'   THEN 'NORMAL' 
	     END AS CONCLUCION_HDL_COLESTEROL,  
	CASE WHEN ab.txtvldlcolesterol IS NOT NULL THEN ab.txtvldlcolesterol ELSE 'INF. NO TOMADA'
	     END  AS VLDL_Colesterol,   
	CASE WHEN ab.txtvldlcolesterol= '' or ab.txtvldlcolesterol= '-' or ab.txtvldlcolesterol= '.'or ab.txtvldlcolesterol= 'N/A' or ab.txtvldlcolesterol IS NULL THEN 'INF. NO TOMADA' 
             WHEN ab.txtvldlcolesterol::decimal = '0' THEN 'INF.NO TOMADA'
	     WHEN ab.txtvldlcolesterol::decimal < '30' THEN 'NORMAL' 
	     WHEN ab.txtvldlcolesterol::decimal >= '30'  THEN 'ANORMAL' 
	     END AS CONCLUCION_VLDL_COLESTEROL, 
	CASE WHEN ab.txttrigliseridos IS NOT NULL THEN ab.txttrigliseridos ELSE '...'
	    END  AS trigliceridos, 
	CASE WHEN ab.txttrigliseridos= '' or ab.txttrigliseridos= '-' or ab.txttrigliseridos= '.'or ab.txttrigliseridos= 'N/A' or ab.txttrigliseridos IS NULL THEN 'INF. NO TOMADA'
	     WHEN ab.txttrigliseridos::decimal < '150' THEN 'NORMAL' 
	     WHEN ab.txttrigliseridos::decimal >= '150'  THEN 'ANORMAL' 
	     END AS CONCLUCION_TRIGLISERIDOS,     
	CASE WHEN ab.txtcreatinina IS NOT NULL THEN ab.txtcreatinina ELSE 'INF. NO TOMADA'
	    END  AS Creatinina, 
	CASE WHEN ab.txtcreatinina= '' or ab.txtcreatinina= '-' or ab.txtcreatinina= '.'or ab.txtcreatinina= 'N/A' or ab.txtcreatinina IS NULL THEN 'INF. NO TOMADA'
	     WHEN ab.txtcreatinina::decimal < '0.8' or ab.txtcreatinina::decimal > '1.4'  THEN 'ANORMAL' 
	     WHEN ab.txtcreatinina::decimal >= '0.8' AND ab.txtcreatinina::decimal <='1.4'   THEN 'NORMAL' 
	     END AS CONCLUCION_CREATININA,  
	CASE WHEN t.cintura is null THEN 'INF. NO TOMADA' ELSE t.cintura END,
	CASE WHEN t.cadera is null THEN 'INF. NO TOMADA' ELSE t.cadera END,
	CASE WHEN t.icc is null THEN 'INF. NO TOMADA' ELSE t.icc END,
	CASE WHEN t.sistolica IS NULL THEN 'INF. NO TOMADA' ELSE t.sistolica END,
	CASE WHEN t.sistolica < '120' THEN 'NORMAL' 
	     WHEN t.sistolica > '119.9' AND t.sistolica < '140' THEN 'PREHIPERTENSION' 
	     WHEN t.sistolica >= '140' AND t.sistolica <'160' THEN 'HTA-1' 
	     WHEN t.sistolica <= '99'  THEN 'NORMAL' 
	     WHEN t.sistolica >= '160' THEN 'HTA-2'
	     WHEN t.sistolica IS NULL THEN 'INF. NO TOMADA'
	     ELSE 'DATOS INSUFICIENTES'   
	     END AS R_SIST,
	CASE WHEN t.diastolica IS NULL THEN 'INF NO TOMADA' ELSE t.diastolica END ,
	CASE WHEN t.diastolica ='100' THEN 'HTA-2'
	     WHEN t.diastolica <'80' THEN 'NORMAL'
	     WHEN t.diastolica >'79.9' AND t.diastolica < '90' THEN 'PREHIPERTENSION' 
	     WHEN t.diastolica >='90' AND t.diastolica < '100' THEN 'HTA-1' 
	     WHEN t.diastolica >='90' THEN 'HTA-1' 
	     WHEN t.diastolica >'100' THEN 'HTA-2'
	     WHEN t.diastolica IS NULL THEN 'INF NO TOMADA'
	     ELSE 'DATOS INSUFICIENTES'  
	     END AS R_DIAST,
        CASE WHEN t.f_cardiaca IS NULL THEN 'INF. NO TOMADA' ELSE t.f_cardiaca END AS FC,
	CASE WHEN t.f_respiratoria IS NULL THEN 'INF. NO TOMADA' ELSE t.f_respiratoria END AS FR,
	CASE WHEN n.razon_empresa !='' THEN 'N/A' END  as PLOMO_SANGRE,
	CASE WHEN n.razon_empresa !='' THEN 'N/A' END  as CADMIO_ORINA,	
	CASE WHEN n.razon_empresa !='' THEN 'N/A' END  as TIOCINATOS_ORINA,
	CASE WHEN ie.n_orden is null THEN 'NO REGISTRO EKG'ELSE  ie.hallazgo END AS EKG_RESULTADO,
	CASE WHEN a.txtobservacionesfm IS NULL THEN 'NO TIENE OBSERVACIONES' ELSE a.txtobservacionesfm END AS ResultadodeEvaluaciones,            
	CASE WHEN lc.chko = 'TRUE' THEN 'O'
	     WHEN lc.chka = 'TRUE' THEN 'A'
	     WHEN lc.chkb = 'TRUE' THEN 'B'
	     WHEN lc.chkab = 'TRUE' THEN 'AB' ELSE '.' END ||''|| 
	CASE WHEN lc.rbrhpositivo = 'TRUE' THEN '+' 
	     WHEN lc.rbrhnegativo = 'TRUE' THEN '-' END AS Grupoyfactor,
	CASE WHEN n.n_orden IS NOT NULL THEN 'N/A' END  as ALERGIAS,
	CASE WHEN cma.p_si1 = 'TRUE' THEN 'Apto'
             WHEN cma.p_no1 = 'TRUE' THEN 'No Apto'
             WHEN cma.n_orden is null  THEN 'NO REGISTRO ALTURA'
             END AS APTITUD_TRAB_ALTURA_BASICO,	
        CASE WHEN ba.chk_si = 'TRUE' THEN 'Apto'
             WHEN ba.chk_apto_r = 'TRUE' THEN 'Apto con Restriccion'
             WHEN ba.chk_no_apto = 'TRUE' THEN 'No Apto'
             WHEN ba.chk_observado = 'TRUE' THEN 'Observado'
             WHEN ba.n_orden is null  THEN 'NO REGISTRO ALTURA'
             END AS APTITUD_TRAB_ALTURA_MINERIA,	
	CASE WHEN bc.chk_si = 'TRUE' THEN 'Apto'
             WHEN bc.chk_apto_r = 'TRUE' THEN 'Apto con Restriccion'
             WHEN bc.chk_observado = 'TRUE' THEN 'Observado'
             WHEN bc.n_orden is null  THEN 'NO REGISTRO APTITUD MANEJO'
             END AS APTITUD_MANEJO,	
	CASE WHEN ob.n_orden is not null THEN 'Observado'
             WHEN ca.chkapto = 'TRUE' THEN 'Apto'
             WHEN ca.chkapto_restriccion = 'TRUE' THEN 'Apto con Restriccion'
	     WHEN ama.chkapto = 'TRUE' THEN 'Apto'
             WHEN ama.chkapto_restriccion = 'TRUE' THEN 'Apto con Restriccion'
             WHEN ca.chkno_apto = 'TRUE' THEN 'No Apto'
             WHEN ama.chkno_apto = 'TRUE' THEN 'No Apto'
             WHEN ama.n_orden IS NULL AND ca.n_orden IS NULL THEN 'NO REGISTRO APTITUD'
              END as APTITUD_OCUP,
	n.n_medico,
	CASE WHEN ip.n_orden is null THEN 'NO REGISTRO EVAL.PSICOLOGICA'ELSE  ip.a_intelectual END ,
	CASE WHEN ip.n_orden is null THEN 'NO REGISTRO EVAL.PSICOLOGICA'ELSE  ip.a_personalidad END ,
	CASE WHEN ip.n_orden is null THEN 'NO REGISTRO EVAL.PSICOLOGICA'ELSE  ip.recomendaciones END ,
	CASE WHEN ip.aprobo_inf ='TRUE' THEN 'APTO'
	     WHEN ip.desaprobo_inf ='TRUE' THEN 'NO APTO'
	     ELSE 'NO REGISTRO EVAL.PSICOLOGICA' END as APTITUDPSICOLOGIA,
	CASE WHEN eme.txtdiagnostico IS NOT NULL THEN eme.txtdiagnostico 
	     WHEN emes.txtconclusiones IS NOT NULL THEN emes.txtconclusiones
	     ELSE 'NO REGISTRO EVAL.MUSC.ESQUELETICA' END AS DIAGNOSTICO_MUSCULO_ESQUELETICO,
	CASE WHEN eme.txtrecomendaciones IS NOT NULL THEN eme.txtrecomendaciones 
	     WHEN emes.txtrecomendaciones IS NOT NULL THEN emes.txtrecomendaciones
	     ELSE 'NO REGISTRO EVAL.MUSC.ESQUELETICA' END AS RECOMENDACION_MUSCULO_ESQUELETICO,
	CASE WHEN ca.txtconclusiones IS NOT NULL THEN ca.txtconclusiones
	     WHEN ama.txtconclusiones IS NOT NULL THEN ama.txtconclusiones
	     ELSE 'NO REGISTRO APTITUD' END AS CONCLUSIONESAPTITUD,
	CASE WHEN ca.txtrecomendaciones IS NOT NULL THEN ca.txtrecomendaciones
	     WHEN ama.txtrecomendaciones IS NOT NULL THEN ama.txtrecomendaciones
             ELSE 'NO REGISTRO APTITUD'END AS RECOMENDACIONESAPTITUD, 
        CASE WHEN ca.atxtrestricciones IS NOT NULL THEN ca.atxtrestricciones
	     WHEN ama.atxtrestricciones IS NOT NULL THEN ama.atxtrestricciones
             ELSE 'NO REGISTRO APTITUD'END AS RESTRICCIONESAPTITUD	
FROM datos_paciente AS d
INNER JOIN n_orden_ocupacional AS n ON (d.cod_pa = n.cod_pa)
LEFT JOIN triaje AS t ON (t.n_orden = n.n_orden)
LEFT JOIN oftalmologia AS o ON (o.n_orden = n.n_orden)
LEFT JOIN oftalmologia_lo AS ol ON (ol.n_orden = n.n_orden)
LEFT JOIN oftalmologia2021 AS ol1 ON (ol1.n_orden = n.n_orden)
LEFT JOIN audiometria_po AS au ON (au.n_orden = n.n_orden)
LEFT JOIN audiometria_2021 AS au1 ON (au1.n_orden = n.n_orden)
LEFT JOIN audiometria_2023 AS au3 ON (au3.n_orden = n.n_orden)
LEFT JOIN funcion_abs AS f ON (f.n_orden = n.n_orden)
LEFT JOIN ficha_audiologica AS fa ON(fa.n_orden = n.n_orden) 
LEFT JOIN informe_electrocardiograma AS ie ON (ie.n_orden = n.n_orden)
LEFT JOIN ex_radiograficos_sanguineos AS er ON (er.n_orden = n.n_orden)
LEFT JOIN radiografia_torax AS rt ON (rt.n_orden = n.n_orden)
LEFT JOIN oit AS oi ON (oi.n_orden = n.n_orden)
LEFT JOIN odontograma AS od ON(od.n_orden = n.n_orden)
LEFT JOIN analisis_bioquimicos AS ab ON(ab.n_orden = n.n_orden)
LEFT JOIN lab_clinico AS lc ON (lc.n_orden = n.n_orden)  
left join anexo7c as a ON (a.n_orden=n.n_orden)
left join anexo_agroindustrial as aa ON (aa.n_orden=n.n_orden)
left join certificado_aptitud_medico_ocupacional as ca ON (ca.n_orden=n.n_orden)
left join aptitud_medico_ocupacional11 as ca1 ON (ca1.n_orden=n.n_orden)
left join aptitud_medico_ocupacional_agro as ama ON (ama.n_orden=n.n_orden)
left join anexo7d as ad ON (ad.n_orden=n.n_orden)
left join anexoc as ac ON (ac.n_orden=n.n_orden)
left join observaciones as ob ON (ob.n_orden=n.n_orden)
left join b_certificado_conduccion as bc ON (bc.n_orden=n.n_orden)
left join b_certificado_altura as ba ON (ba.n_orden=n.n_orden)   
left join certificacion_medica_altura as cma ON (cma.n_orden=n.n_orden)
Left join informe_psicologico as ip ON (ip.n_orden=n.n_orden)
Left join evaluacion_musculo_esqueletica as eme ON (eme.n_orden=n.n_orden)
Left join evaluacion_musculo_esqueletica2021 as emes ON (emes.n_orden=n.n_orden)
left join empresas as ep on n.razon_empresa=ep.razon_empresa 
left join contratas as ct on n.razon_contrata=ct.razon_contrata 
WHERE  (ep.ruc_empresa=ruc_empresa_param or ct.ruc_contrata=ruc_contrata_param) AND n.fecha_apertura_po >= CAST(f_inicio AS DATE) AND n.fecha_apertura_po <= CAST(f_fin AS DATE) 
order by n.n_orden asc;

END $function$
;

-- Permissions

ALTER FUNCTION public.sp_matriz_general(text, text, text, text) OWNER TO pierola;
GRANT ALL ON FUNCTION public.sp_matriz_general(text, text, text, text) TO pierola;

-- DROP FUNCTION public.sp_matriz_salud_ohla(text, text, text);

CREATE OR REPLACE FUNCTION public.sp_matriz_salud_ohla(ruc_ohla text, f_inicio text, f_fin text)
 RETURNS TABLE(n bigint, nombres text, razon_contrata text, razon_empresa text, txtobserv1 text, txtobserv2 text, genero text, dni integer, fecha_nacimiento_pac text, edad_pac integer, rango text, fecha_inicio text, fecha_salida text, estado text, clinica text, tipodeemo text, ultimoemo text, fechavence text, plazodias text, status text, entregado text, accion text, txtantecedentesfamiliares text, txtantecedentespersonales text, anteceenfermedad text, alergias text, presionarterial text, peso text, talla text, imc text, r_imc text, p_abdominal text, fr text, fc text, temperatura text, sat_02 text, hemoglobina text, conclucion_hemoglobina text, txthematocrito text, txtleucocitosematologia text, txthematiesematologia text, txteosinofiosematologia text, txtlinfocitosematologia text, txtmonocitosematologia text, txtabastonados text, promielocitos text, mieolocitosis text, txtsegmentadosematologia text, vcm text, hcm text, chcm text, txtplaquetas text, txtvsg text, grupo text, factor text, glucosa text, valores_glucosa text, urea text, creatinina text, conclucion_creatinina text, acidourico text, total text, dir text, ind text, fosfatasaalcaina text, total2 text, alb text, glb text, ggtp text, colesterol text, trigliceridos text, ldl_colesterol text, hdl_colesterol text, analisis_cole text, analisis_trigli text, tgo text, tgp text, txtleucocitossu text, txthematiessu text, txtcristalessu text, txtcelepitelialessu text, txtotrossu text, txtbacteriassu text, txtcocaina text, txtmarihuana text, estasis text, metanfetaminas text, benzodiacepinas text, anfetamina text, combinado text, torax text, ekg_resultado text, lumbar text, prueba_esfuerso text, riesgo_coronario text, cerca_od text, cerca_oi text, lejos_od text, lejos_oi text, cerca_corregida_od text, cerca_corregida_oi text, lejos_corregida_od text, lejos_corregida_oi text, ishihara text, campimetria text, fondo_ojo text, nictometria text, estereopsis text, conc_diagnostica text, audiometria text, espirometria text, txtcariadasoturar text, txtobturacionesefectuadas text, txtausentes text, conclusionodo text, aptitudpsicologia text, ectoscopia text, diagnostico_musculo_esqueletico text, osteomuscular text, neurologico text, dermatologico text, espacios_confinados text, aptitud_trab_altura_mineria text, anexo16a text, anexo10 text, resultadodeevaluaciones text, conclusionesaptitud text, recomendacionesaptitud text, aptitud_ocup text, restriccionesaptitud text, seguimiento_observaciones text)
 LANGUAGE plpgsql
AS $function$
BEGIN
	RETURN QUERY
     SELECT CAST(n.n_orden AS bigint) AS N, TRIM(d.apellidos_pa) || ' ' || TRIM(nombres_pa)  NOMBRES,n.razon_empresa,n.razon_contrata,n.txtobserv1,n.txtobserv2, 
       CASE WHEN d.sexo_pa='F' THEN 'FEMENINO' ELSE 'MASCULINO' END AS GENERO,CAST(d.cod_pa AS integer) AS DNI,
	cast(d.fecha_nacimiento_pa as text) AS fecha_nacimiento_pac,
        cast(obtener_edad(d.fecha_nacimiento_pa,n.fecha_apertura_po)as integer) AS edad_pac,
       CASE WHEN obtener_edad(d.fecha_nacimiento_pa,n.fecha_apertura_po)<=24 then '14-24'
	    WHEN obtener_edad(d.fecha_nacimiento_pa,n.fecha_apertura_po)<=59 then '25-59'
	    WHEN obtener_edad(d.fecha_nacimiento_pa,n.fecha_apertura_po)<=64 then '60-64'
	    ELSE '>=65' END rango,
	cast(fecha_apertura_po as text) AS fecha_inicio,
	CASE WHEN n.n_orden is not null THEN '' END  AS fecha_salida,
	CASE WHEN n.n_orden is not null THEN '' END  AS ESTADO,
	CASE WHEN n.n_orden is not null THEN 'HORIZONTE MEDIC' END  AS CLINICA,
        n.nom_examen AS TIPODEEMO,
        CASE WHEN n.n_orden is not null THEN '' END  AS ULTIMOEMO,
        CAST((CASE WHEN ca.fecha_hasta IS NOT NULL THEN ca.fecha_hasta 
	     WHEN ama.fecha_hasta IS NOT NULL THEN ama.fecha_hasta
	      END) AS TEXT) AS FECHAVENCE,
        CASE WHEN n.n_orden is not null THEN '' END  AS PLAZODIAS,
        CASE WHEN n.n_orden is not null THEN '' END  AS STATUS,
        CASE WHEN n.n_orden is not null THEN '' END  AS ENTREGADO,
        CASE WHEN n.n_orden is not null THEN '' END  AS ACCION,
	a.txtantecedentesfamiliares,a.txtantecedentespersonales, 
        CASE WHEN n.n_orden is not null THEN '' END  AS ANTECENFERMEDAD,
        CASE WHEN apa.chk1='TRUE' THEN 'SI' ELSE 'NIEGA' END  AS ALERGIAS,
	t.sistolica ||'/'|| t.diastolica AS PRESIONARTERIAL,
        CASE WHEN t.peso is null THEN 'INF. NO TOMADA' ELSE t.peso END,
	CASE WHEN t.talla is null THEN 'INF. NO TOMADA' ELSE t.talla END,
	CASE WHEN t.imc is null THEN 'INF. NO TOMADA' ELSE t.imc END,
	CASE WHEN t.imc < '16' THEN 'MUY BAJO PESO'
	     WHEN t.imc > '16.9' AND t.imc < '18.5' THEN 'PESO BAJO'
	     WHEN t.imc > '18.4' AND t.imc < '25'  THEN 'NORMOPESO'
	     WHEN t.imc > '24.9' AND t.imc < '30'  THEN 'SOBREPESO'   
	     WHEN t.imc > '29.9' AND t.imc < '35' THEN 'OBESIDAD'
	     WHEN t.imc > '34.9' AND t.imc < '40' THEN 'OBESIDAD II'
	     WHEN t.imc > '39.9' THEN 'OBESIDAD MORBIDA'
	     WHEN t.imc is null THEN 'SIN FICHA 7C'
	     END  AS R_IMC,
	CASE WHEN t.cintura is null THEN 'INF. NO TOMADA' ELSE t.cintura END AS P_ABDOMINAL,
	CASE WHEN t.f_respiratoria IS NULL THEN 'INF. NO TOMADA' ELSE t.f_respiratoria END AS FR,
	CASE WHEN t.f_cardiaca IS NULL THEN 'INF. NO TOMADA' ELSE t.f_cardiaca END AS FC,
        t.temperatura,t.sat_02,
	CASE WHEN lc.txthemoglobina IS NULL THEN 'INF. NO TOMADA' ELSE lc.txthemoglobina END AS Hemoglobina ,
	CASE WHEN (lc.txthemoglobina < '12' or lc.txthemoglobina > '20')AND d.sexo_pa='M' THEN 'ANORMAL' 
	     WHEN (lc.txthemoglobina < '12' or lc.txthemoglobina > '20')AND d.sexo_pa='F' THEN 'ANORMAL' 
	     WHEN (lc.txthemoglobina >= '12' AND lc.txthemoglobina <='20')AND d.sexo_pa='M'THEN 'NORMAL' 
	     WHEN (lc.txthemoglobina >= '12' AND lc.txthemoglobina <='20')AND d.sexo_pa='F' THEN 'NORMAL' 
	     WHEN lc.txthemoglobina IS NULL THEN 'INF. NO TOMADA'
	     END AS CONCLUCION_HEMOGLOBINA ,lc.txthematocrito,lc.txtleucocitosematologia,lc.txthematiesematologia,
        lc.txteosinofiosematologia,lc.txtlinfocitosematologia,lc.txtmonocitosematologia,lc.txtabastonados,
        CASE WHEN n.n_orden is not null THEN '' END  AS promielocitos,
        CASE WHEN n.n_orden is not null THEN '' END  AS mieolocitosis,lc.txtsegmentadosematologia,
	 CASE WHEN n.n_orden is not null THEN '' END  AS vcm,
	  CASE WHEN n.n_orden is not null THEN '' END  AS hcm,
	   CASE WHEN n.n_orden is not null THEN '' END  AS chcm,lc.txtplaquetas,lc.txtvsg,
	   CASE WHEN lc.chko = 'TRUE' THEN 'O'
	     WHEN lc.chka = 'TRUE' THEN 'A'
	     WHEN lc.chkb = 'TRUE' THEN 'B'
	     WHEN lc.chkab = 'TRUE' THEN 'AB' ELSE '.' END as grupo,
	CASE WHEN lc.rbrhpositivo = 'TRUE' THEN 'POSITIVO' 
	     WHEN lc.rbrhnegativo = 'TRUE' THEN 'NEGATIVO' END AS factor,

	CASE WHEN lc.txtglucosabio IS NULL THEN 'INF. NO TOMADA' ELSE lc.txtglucosabio END AS Glucosa,
	CASE WHEN lc.txtglucosabio = 'N/A' OR lc.txtglucosabio = ''OR lc.txtglucosabio = '-'OR lc.txtglucosabio = '.' or lc.txtglucosabio is null THEN 'INF. NO TOMADA'
             WHEN lc.txtglucosabio::decimal <'70'  THEN 'HIPOGLUCEMIA' 
             WHEN lc.txtglucosabio::decimal >='70' AND lc.txtglucosabio::decimal<='110' THEN 'NORMO GLUCEMIA' 
	     ELSE 'HIPERGLICEMIA' END AS VALORES_GLUCOSA,
	CASE WHEN n.n_orden is not null THEN '' END  AS UREA,
	CASE WHEN ab.txtcreatinina IS NOT NULL THEN ab.txtcreatinina ELSE 'INF. NO TOMADA'
	    END  AS Creatinina, 
	CASE WHEN ab.txtcreatinina= '' or ab.txtcreatinina= '-' or ab.txtcreatinina= '.'or ab.txtcreatinina= 'N/A' or ab.txtcreatinina IS NULL THEN 'INF. NO TOMADA'
	     WHEN ab.txtcreatinina::decimal < '0.8' or ab.txtcreatinina::decimal > '1.4'  THEN 'ANORMAL' 
	     WHEN ab.txtcreatinina::decimal >= '0.8' AND ab.txtcreatinina::decimal <='1.4'   THEN 'NORMAL' 
	     END AS CONCLUCION_CREATININA,  acu.txtresultado as acidourico,
	CASE WHEN ph.txtr_bilirrtotal is not null THEN ph.txtr_bilirrtotal END  AS total,
	CASE WHEN ph.txtr_bilirrdirecta is not null THEN ph.txtr_bilirrdirecta END  AS dir,
	CASE WHEN ph.txtr_bilirrindirecta is not null THEN ph.txtr_bilirrindirecta END  AS ind,
	CASE WHEN ph.txtr_fosfalcalina is not null THEN ph.txtr_fosfalcalina END  AS fosfatasaalcaina,
	CASE WHEN ph.txtr_protetotales is not null THEN ph.txtr_protetotales  END  AS total2,
	CASE WHEN ph.txtr_albumina is not null THEN ph.txtr_albumina END  AS alb,
	CASE WHEN ph.txtr_globulina is not null THEN ph.txtr_globulina END  AS glb,
	CASE WHEN ph.txtr_ggt is not null THEN ph.txtr_ggt END  AS ggtp,
	CASE WHEN ab.txtcolesterol IS NOT NULL THEN ab.txtcolesterol ELSE 'INF. NO TOMADA'
	     END  AS Colesterol,   
	CASE WHEN ab.txttrigliseridos IS NOT NULL THEN ab.txttrigliseridos ELSE '...'
	    END  AS trigliceridos, 	         
	CASE WHEN ab.txtldlcolesterol IS NOT NULL THEN ab.txtldlcolesterol ELSE 'INF. NO TOMADA'
	     END  AS LDL_COLESTEROL,  
	CASE WHEN ab.txthdlcolesterol IS NOT NULL THEN ab.txthdlcolesterol ELSE 'INF. NO TOMADA'
	     END  AS HDL_Colesterol,   	     
	CASE WHEN ab.txtldlcolesterol= '' or ab.txtldlcolesterol= '-' or ab.txtldlcolesterol= '.'or ab.txtldlcolesterol= 'N/A'or ab.txttrigliseridos= '' or ab.txttrigliseridos= '-' or ab.txttrigliseridos= '.'or ab.txttrigliseridos= 'N/A'
	 or ab.txtldlcolesterol IS NULL THEN 'INF. NO TOMADA' 
	
	     WHEN ab.txttrigliseridos::decimal = '200' THEN 'HIPERTRIGLICPERIDEMIA'
	     ELSE 'DISPLIDEMIA MIXTA' END AS Analisis_cole, 	   
	CASE WHEN ab.txttrigliseridos= '' or ab.txttrigliseridos= '-' or ab.txttrigliseridos= '.'or ab.txttrigliseridos= 'N/A' or ab.txttrigliseridos IS NULL THEN 'INF. NO TOMADA' 
             WHEN ab.txttrigliseridos::decimal = '0' THEN 'INF.NO TOMADA'
	     WHEN ab.txttrigliseridos::decimal < '150' THEN 'NORMAL' 
	     WHEN ab.txttrigliseridos::decimal >= '150' and ab.txttrigliseridos::decimal < '200'  THEN 'NORMAL ALTO' 
	     WHEN ab.txttrigliseridos::decimal >= '200' and ab.txttrigliseridos::decimal < '500'  THEN 'ELEVADO' 
	     WHEN ab.txttrigliseridos::decimal >= '500' and ab.txttrigliseridos::decimal < '1000'  THEN 'MUY ELEVADO' 
	     ELSE 'EXCESIVO' END AS Analisis_trigli, 
	CASE WHEN ph.txtr_tgo is not null THEN ph.txtr_tgo END  AS tgo,  
        CASE WHEN ph.txtr_tgp is not null THEN ph.txtr_tgp END  AS tgp,
	lc. txtleucocitossu,lc.txthematiessu,lc.txtcristalessu,lc.txtcelepitelialessu,lc.txtotrossu,
	lc.txtbacteriassu,lc.txtcocaina,lc.txtmarihuana,
	CASE WHEN n.n_orden is not null THEN '' END  AS estasis,
	CASE WHEN tox.txtr_methanfetamina is not null THEN tox.txtr_methanfetamina END  AS Metanfetaminas,
	CASE WHEN tox.txtr_benzodiacepina is not null THEN tox.txtr_benzodiacepina END  AS Benzodiacepinas,
        CASE WHEN tox.txtr_anfetamina is not null THEN tox.txtr_anfetamina END  AS Anfetamina,
	CASE WHEN n.n_orden is not null THEN '' END  AS Combinado,
	CASE WHEN rt.n_orden is null then 'NO SE REGISTRO INF. RADIGR.' else rt.txtconclusionesradiograficas end as torax,
	CASE WHEN ie.n_orden is null THEN 'NO REGISTRO EKG'ELSE  ie.hallazgo END AS EKG_RESULTADO,
	CASE WHEN n.n_orden is not null THEN '.' END  AS lumbar,
	CASE WHEN n.n_orden is not null THEN '.' END  AS prueba_esfuerso,
	CASE WHEN n.n_orden is not null THEN '.' END  AS riesgo_coronario,
	o.v_cerca_s_od AS Cerca_OD,
	o.v_cerca_s_oi AS Cerca_OI,
	o.v_lejos_s_od AS Lejos_OD,
	o.v_lejos_s_oi AS Lejos_OI,
	CASE WHEN o.v_cerca_c_od ='00' THEN ol.v_cerca_c_od ELSE o.v_cerca_c_od END 
	AS Cerca_Corregida_OD,
	CASE WHEN o.v_cerca_c_oi ='00' THEN ol.v_cerca_c_oi ELSE o.v_cerca_c_oi END
	AS Cerca_Corregida_OI,     
	CASE WHEN o.v_lejos_c_od ='00' THEN ol.v_lejos_c_od ELSE o.v_lejos_c_od END 
	AS Lejos_Corregida_OD,
	CASE WHEN o.v_lejos_c_oi ='00' THEN ol.v_lejos_c_oi ELSE o.v_lejos_c_oi END
	AS Lejos_Corregida_OI,
	CASE WHEN oft.rbtecishihara_normal='TRUE' THEN 'NORMAL'
	     ELSE 'ANORMAL' END AS Ishihara,
	CASE WHEN n.n_orden is not null THEN 'NO APLICA' END  AS Campimetria,
	
	CASE WHEN oft.rbfonormalod='TRUE' THEN 'OD: NORMAL'
	     WHEN oft.rbfoanormalod='TRUE' THEN 'OD: ANORMAL' ELSE 'N/A' END ||' '||
	CASE WHEN oft.rbfonormaloi='TRUE' THEN 'OI: NORMAL'
	     WHEN oft.rbfoanormaloi='TRUE' THEN 'OI: ANORMAL' ELSE 'N/A' END AS Fondo_ojo,
	CASE WHEN n.n_orden is not null THEN 'NO APLICA' END  AS Nictometria,
	CASE WHEN n.n_orden is not null THEN 'NO APLICA' END  AS Estereopsis,
	CASE WHEN ol.e_oculares is null THEN o.e_oculares ELSE ol.e_oculares END || ' '||
	CASE WHEN ol.e_oculares1 IS NULL THEN o.e_oculares1 ELSE ol.e_oculares1  END as Conc_diagnostica,
	CASE WHEN au.o_d_500='N/A' OR au.o_d_500 IS NULL THEN 'NO SE REALIZO AUDIOMETRIA' ELSE au.diagnostico END Audiometria,
	CASE WHEN f.fvc='N/A' or f.n_orden is null THEN 'NO SE REALIZO ESPIROMETRIA' ELSE a.txtconclusion END as Espirometria,
	cast(odo.txtcariadasoturar as text) as txtcariadasoturar,cast(odo.txtobturacionesefectuadas as text) as txtobturacionesefectuadas,
	cast(odo.txtausentes as text) as txtausentes,
	CASE WHEN odo.txtobservaciones is null THEN 'NO PASO EXAMEN ODONTOLOGICO' else odo.txtobservaciones  END  AS conclusionodo,
	CASE WHEN ip.aprobo_inf ='TRUE' THEN 'APTO'
	     WHEN ip.desaprobo_inf ='TRUE' THEN 'NO APTO'
	     ELSE 'NO REGISTRO EVAL.PSICOLOGICA' END as APTITUDPSICOLOGIA,
	CASE WHEN n.n_orden is not null THEN 'NO APLICA' END  AS Ectoscopia,
	CASE WHEN eme.txtdiagnostico IS NOT NULL THEN eme.txtdiagnostico 
	     WHEN emes.txtconclusiones IS NOT NULL THEN emes.txtconclusiones
	     ELSE 'NO REGISTRO EVAL.MUSC.ESQUELETICA' END AS DIAGNOSTICO_MUSCULO_ESQUELETICO,
	CASE WHEN n.n_orden is not null THEN 'NO APLICA' END  AS osteomuscular,
	CASE WHEN n.n_orden is not null THEN 'NO APLICA' END  AS neurologico,
	CASE WHEN n.n_orden is not null THEN 'NO APLICA' END  AS dermatologico,
	CASE WHEN n.n_orden is not null THEN 'NO APLICA' END  AS espacios_confinados,
	 CASE WHEN ba.chk_si = 'TRUE' THEN 'Apto'
             WHEN ba.chk_apto_r = 'TRUE' THEN 'Apto con Restriccion'
             WHEN ba.chk_no_apto = 'TRUE' THEN 'No Apto'
             WHEN ba.chk_observado = 'TRUE' THEN 'Observado'
             WHEN ba.n_orden is null  THEN 'NO REGISTRO ALTURA'
             END AS APTITUD_TRAB_ALTURA_MINERIA,
	CASE WHEN n.n_orden is not null THEN 'NO APLICA' END  AS anexo16a,
	CASE WHEN n.n_orden is not null THEN 'NO APLICA' END  AS anexo10,	
	CASE WHEN a.txtobservacionesfm IS NULL THEN 'NO TIENE OBSERVACIONES' ELSE a.txtobservacionesfm END AS ResultadodeEvaluaciones,    
	CASE WHEN ca.txtconclusiones IS NOT NULL THEN ca.txtconclusiones
	     ELSE 'NO REGISTRO APTITUD' END AS CONCLUSIONESAPTITUD,
	CASE WHEN ca.txtrecomendaciones IS NOT NULL THEN ca.txtrecomendaciones
             ELSE 'NO REGISTRO APTITUD'END AS RECOMENDACIONESAPTITUD, 
        CASE WHEN ob.n_orden is not null THEN 'Observado'
             WHEN ca.chkapto = 'TRUE' THEN 'Apto'
             WHEN ap1.chkapto = 'TRUE' THEN 'Apto'
             WHEN ca.chkapto_restriccion = 'TRUE' THEN 'Apto con Restriccion'
             WHEN ap1.chkapto_restriccion = 'TRUE' THEN 'Apto con Restriccion'
             WHEN ca.chkno_apto = 'TRUE' THEN 'No Apto'
             WHEN ap1.chkno_apto = 'TRUE' THEN 'No Apto'
             WHEN ca.chkevaluado = 'TRUE' THEN 'Evaluado'
             WHEN ca.chkconobservacion = 'TRUE' THEN 'Con Observaciones'
             WHEN fi.n_orden IS NOT NULL THEN 'INTERCONSULTA PENDIENTE'||':'||string_agg (fi.especialidad,'-') 
             WHEN ca.n_orden IS NULL THEN 'NO REGISTRO APTITUD'
              END as APTITUD_OCUP,
        CASE WHEN ca.atxtrestricciones IS NOT NULL THEN ca.atxtrestricciones
             ELSE 'NO REGISTRO APTITUD'END AS RESTRICCIONESAPTITUD,
	CASE WHEN n.n_orden is not null THEN 'NO APLICA' END  AS SEGUIMIENTO_OBSERVACIONES
FROM datos_paciente AS d
INNER JOIN n_orden_ocupacional AS n ON (d.cod_pa = n.cod_pa)
LEFT JOIN triaje AS t ON (t.n_orden = n.n_orden)
LEFT JOIN oftalmologia AS o ON (o.n_orden = n.n_orden)
LEFT JOIN oftalmologia_lo AS ol ON (ol.n_orden = n.n_orden)
LEFT JOIN audiometria_po AS au ON (au.n_orden = n.n_orden)
LEFT JOIN funcion_abs AS f ON (f.n_orden = n.n_orden)
LEFT JOIN ficha_audiologica AS fa ON(fa.n_orden = n.n_orden) 
LEFT JOIN informe_electrocardiograma AS ie ON (ie.n_orden = n.n_orden)
LEFT JOIN ex_radiograficos_sanguineos AS er ON (er.n_orden = n.n_orden)
LEFT JOIN radiografia_torax AS rt ON (rt.n_orden = n.n_orden)
LEFT JOIN oit AS oi ON (oi.n_orden = n.n_orden)
LEFT JOIN odontograma AS od ON(od.n_orden = n.n_orden)
LEFT JOIN analisis_bioquimicos AS ab ON(ab.n_orden = n.n_orden)
LEFT JOIN lab_clinico AS lc ON (lc.n_orden = n.n_orden)  
left join anexo7c as a ON (a.n_orden=n.n_orden)
left join anexo_agroindustrial as aa ON (aa.n_orden=n.n_orden)
left join aptitud_medico_ocupacional11 as ca ON (ca.n_orden=n.n_orden)
left join aptitud_medico_ocupacional_agro as ama ON (ama.n_orden=n.n_orden)
left join anexo7d as ad ON (ad.n_orden=n.n_orden)
left join anexoc as ac ON (ac.n_orden=n.n_orden)
left join observaciones as ob ON (ob.n_orden=n.n_orden)
left join b_certificado_conduccion as bc ON (bc.n_orden=n.n_orden)
left join b_certificado_altura as ba ON (ba.n_orden=n.n_orden)   
left join certificacion_medica_altura as cma ON (cma.n_orden=n.n_orden)
Left join informe_psicologico as ip ON (ip.n_orden=n.n_orden)
Left join evaluacion_musculo_esqueletica as eme ON (eme.n_orden=n.n_orden)
Left join evaluacion_musculo_esqueletica2021 as emes ON (emes.n_orden=n.n_orden)
Left join ac_bioquimica2022 as acu ON (acu.n_orden=n.n_orden)
Left join toxicologia as tox ON (tox.n_orden=n.n_orden)
Left join oftalmologia2021 as oft ON (oft.n_orden=n.n_orden)
Left join odontograma as odo ON (odo.n_orden=n.n_orden)
Left join perfil_hepatico as ph ON (ph.n_orden=n.n_orden)
Left join antecedentes_patologicos as apa ON (apa.n_orden=n.n_orden)
left join certificado_aptitud_medico_ocupacional as ap1 ON(ap1.n_orden=n.n_orden)
LEFT join ficha_interconsulta as fi ON (n.n_orden=fi.n_orden)
left join empresas as ep on n.razon_empresa=ep.razon_empresa 
	WHERE  ep.ruc_empresa=ruc_ohla AND n.fecha_apertura_po >= CAST(f_inicio AS DATE) AND n.fecha_apertura_po <= CAST(f_fin AS DATE) 
group by NOMBRES,n.razon_empresa,n.razon_contrata,n.txtobserv1,n.txtobserv2, N, 
       GENERO,DNI,fecha_nacimiento_pac,edad_pac,rango,fecha_inicio, fecha_salida,ESTADO,CLINICA,TIPODEEMO,ULTIMOEMO,FECHAVENCE,PLAZODIAS,STATUS,ENTREGADO, ACCION,
	a.txtantecedentesfamiliares,a.txtantecedentespersonales,  ANTECENFERMEDAD, ALERGIAS, PRESIONARTERIAL,t.peso,t.talla , t.imc,R_IMC, P_ABDOMINAL, FR, FC,
        t.temperatura,t.sat_02, Hemoglobina , CONCLUCION_HEMOGLOBINA ,lc.txthematocrito,lc.txtleucocitosematologia,lc.txthematiesematologia,
        lc.txteosinofiosematologia,lc.txtlinfocitosematologia,lc.txtmonocitosematologia,lc.txtabastonados, promielocitos, mieolocitosis,
        lc.txtsegmentadosematologia,vcm, hcm,chcm,lc.txtplaquetas,lc.txtvsg,grupo,factor, Glucosa,VALORES_GLUCOSA,UREA,Creatinina,CONCLUCION_CREATININA,  
        acidourico, total,dir,ind,fosfatasaalcaina,total2,alb,glb,ggtp, Colesterol, trigliceridos, LDL_COLESTEROL,  HDL_Colesterol, Analisis_cole, 
        Analisis_trigli,  tgo, tgp,lc. txtleucocitossu,lc.txthematiessu,lc.txtcristalessu,lc.txtcelepitelialessu,lc.txtotrossu,lc.txtbacteriassu,lc.txtcocaina,
        lc.txtmarihuana,estasis,Metanfetaminas,Benzodiacepinas,Anfetamina,Combinado,torax,EKG_RESULTADO,lumbar,prueba_esfuerso,riesgo_coronario,
	Cerca_OD,Cerca_OI, Lejos_OD, Lejos_OI,Cerca_Corregida_OD, Cerca_Corregida_OI, Lejos_Corregida_OD,Lejos_Corregida_OI,Ishihara, Campimetria,Fondo_ojo,
	Nictometria, Estereopsis, Conc_diagnostica, Audiometria, Espirometria,odo.txtcariadasoturar,odo.txtobturacionesefectuadas,odo.txtausentes, conclusionodo,
	 APTITUDPSICOLOGIA,Ectoscopia,DIAGNOSTICO_MUSCULO_ESQUELETICO,osteomuscular,neurologico, dermatologico,espacios_confinados, APTITUD_TRAB_ALTURA_MINERIA,
	 ResultadodeEvaluaciones,CONCLUSIONESAPTITUD,RECOMENDACIONESAPTITUD,RESTRICCIONESAPTITUD, SEGUIMIENTO_OBSERVACIONES, ob.n_orden,ca.chkapto, ap1.chkapto, 
	 ca.chkapto_restriccion, ap1.chkapto_restriccion,ca.chkno_apto, ap1.chkno_apto, ca.chkevaluado,ca.chkconobservacion,fi.n_orden, ca.n_orden,n.n_orden 
order by n.n_orden asc;

END $function$
;

-- Permissions

ALTER FUNCTION public.sp_matriz_salud_ohla(text, text, text) OWNER TO pierola;
GRANT ALL ON FUNCTION public.sp_matriz_salud_ohla(text, text, text) TO pierola;

-- DROP FUNCTION public.sp_pacientesgrid(text, int4, int4);

CREATE OR REPLACE FUNCTION public.sp_pacientesgrid(p_filtro text, p_cantidad integer DEFAULT 10, p_inicio integer DEFAULT 0)
 RETURNS TABLE(id integer, nombres_pa text, apellidos_pa text)
 LANGUAGE plpgsql
AS $function$
begin
	RETURN query
	
SELECT b.cod_pa as id,
        b.nombres_pa,
        b.apellidos_pa
FROM n_orden_ocupacional a
inner join datos_paciente b on b.cod_pa=a.cod_pa
inner join contratas c on a.razon_contrata=c.razon_contrata
group by b.cod_pa,
        b.nombres_pa,
        b.apellidos_pa;
	

end;
$function$
;

-- Permissions

ALTER FUNCTION public.sp_pacientesgrid(text, int4, int4) OWNER TO postgres;
GRANT ALL ON FUNCTION public.sp_pacientesgrid(text, int4, int4) TO postgres;

-- DROP FUNCTION public.sp_pacientesgrid_v2(text, text, int4, int4);

CREATE OR REPLACE FUNCTION public.sp_pacientesgrid_v2(p_filtro text DEFAULT ''::text, p_ordenadopor text DEFAULT ''::text, p_cantidad integer DEFAULT 0, p_inicio integer DEFAULT 0)
 RETURNS TABLE(id integer, nombres_pa text, apellidos_pa text)
 LANGUAGE plpgsql
AS $function$
DECLARE
	v_sql text = '';
	v_select text = '';
	v_order text = '';
	v_limit text = '';
	v_group_by text = '';
begin
	
	
	v_select:='SELECT b.cod_pa as id,
			b.nombres_pa,
			b.apellidos_pa
	FROM n_orden_ocupacional a
	inner join datos_paciente b on b.cod_pa=a.cod_pa
	inner join contratas c on a.razon_contrata=c.razon_contrata';
	
-- GROUP BY	
	v_group_by:='group by b.cod_pa,	b.nombres_pa,b.apellidos_pa';
	
-- ORDEN
	IF (CHAR_LENGTH(TRIM(p_ordenadopor)) > 0) THEN
		v_order := ' ORDER BY ' || p_ordenadopor;
	END IF;

-- LIMITE	
	IF ( p_cantidad > 0 AND p_inicio >= 0 ) THEN
		v_limit := ' LIMIT  ' || p_cantidad || ' OFFSET ' || p_inicio;
	END IF;	
	
-- SQL	
	v_sql := v_select || CASE WHEN CHAR_LENGTH(TRIM(p_filtro))>0 THEN ' WHERE ' ELSE '' END || p_filtro||' '|| v_group_by || v_order || v_limit;
	RAISE NOTICE 'Query: %', v_sql;

-- EJECUCION QUERY
	RETURN QUERY
	EXECUTE v_sql;	

end;
$function$
;

-- Permissions

ALTER FUNCTION public.sp_pacientesgrid_v2(text, text, int4, int4) OWNER TO postgres;
GRANT ALL ON FUNCTION public.sp_pacientesgrid_v2(text, text, int4, int4) TO postgres;

-- DROP FUNCTION public.sp_pruebagrid(int4, int4, text);

CREATE OR REPLACE FUNCTION public.sp_pruebagrid(pagina integer, regxpag integer, rznsocial text)
 RETURNS TABLE(razon_contrata text, nombres_pa text, apellidos_pa text, fecha_apertura_po date, estado_ex text, nom_examen text, cargo_de text, area_o text)
 LANGUAGE plpgsql
AS $function$

BEGIN        
  FOR razon_contrata, nombres_pa,apellidos_pa,fecha_apertura_po,estado_ex,nom_examen,cargo_de,area_o IN 

    	SELECT a.razon_contrata,b.nombres_pa,b.apellidos_pa,a.fecha_apertura_po,a.estado_ex,a.nom_examen,a.cargo_de,a.area_o
		FROM n_orden_ocupacional a
		inner join datos_paciente b on b.cod_pa=a.cod_pa
		where a.razon_empresa=rznsocial
		limit 1
  LOOP

    RETURN NEXT;

  END LOOP;

  RETURN;

END;

$function$
;

-- Permissions

ALTER FUNCTION public.sp_pruebagrid(int4, int4, text) OWNER TO postgres;
GRANT ALL ON FUNCTION public.sp_pruebagrid(int4, int4, text) TO postgres;

-- DROP FUNCTION public.sp_pruebagrid(text);

CREATE OR REPLACE FUNCTION public.sp_pruebagrid(rznsocial text)
 RETURNS TABLE(razon_contrata text, nombres_pa text, apellidos_pa text, fecha_apertura_po date, estado_ex text, nom_examen text, cargo_de text, area_o text)
 LANGUAGE plpgsql
AS $function$

BEGIN        
  FOR razon_contrata, nombres_pa,apellidos_pa,fecha_apertura_po,estado_ex,nom_examen,cargo_de,area_o IN 

    	SELECT a.razon_contrata,b.nombres_pa,b.apellidos_pa,a.fecha_apertura_po,a.estado_ex,a.nom_examen,a.cargo_de,a.area_o
		FROM n_orden_ocupacional a
		inner join datos_paciente b on b.cod_pa=a.cod_pa
		where a.razon_empresa=rznsocial
		limit 1
  LOOP

    RETURN NEXT;

  END LOOP;

  RETURN;

END;

$function$
;

